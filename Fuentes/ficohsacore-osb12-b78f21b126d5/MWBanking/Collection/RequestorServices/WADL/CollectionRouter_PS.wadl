<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns0="http://www.ficohsa.com.hn/middleware.services/commonTypes" xmlns:inp1="http://www.ficohsa.com.hn/middleware.services/getCollectionTypes" xmlns="http://wadl.dev.java.net/2009/02" xmlns:pyc="http://www.ficohsa.com.hn/middleware.services/paymentCollectionTypes">
   <doc title="CollectionRouter_PS">RestService</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/getCollectionTypes" schemaLocation="../XSD/getCollectionTypes.xsd"/>
        </xsd:schema>
      <xsd:schema>
            <xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/paymentCollectionTypes" schemaLocation="../XSD/paymentCollectionTypes.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/get-collection">
         <method name="POST" soa:wsdlOperation="getCollection">
            <request>
               <representation mediaType="application/json" element="cns:getCollectionRequest" xmlns:cns="http://www.ficohsa.com.hn/middleware.services/getCollectionTypes"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:getCollectionResponse" xmlns:cns="http://www.ficohsa.com.hn/middleware.services/getCollectionTypes"/>
            </response>
         </method>
      </resource>
      <resource path="/payment-collection">
         <method name="POST" soa:wsdlOperation="paymentCollection">
            <request>
               <representation mediaType="application/json" element="cns:paymentCollectionRequest" xmlns:cns="http://www.ficohsa.com.hn/middleware.services/paymentCollectionTypes"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:paymentCollectionResponse" xmlns:cns="http://www.ficohsa.com.hn/middleware.services/paymentCollectionTypes"/>
            </response>
         </method>
      </resource>
   </resources>
</application>