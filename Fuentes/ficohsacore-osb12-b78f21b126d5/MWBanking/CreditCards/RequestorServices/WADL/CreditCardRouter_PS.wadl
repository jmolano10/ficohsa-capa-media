    <resource path="/interest-rate">
      <method name="GET" soa:wsdlOperation="getInterestRate">
        <request soa:paramElement="pns:getInterestRateRequest"
                 xmlns:pns="http://www.ficohsa.com.hn/middleware.services/getInterestRateTypes">
          <param name="SourceBank" style="query" soa:expression="$msg.request/ns0:SourceBank" type="xsd:string"/>
