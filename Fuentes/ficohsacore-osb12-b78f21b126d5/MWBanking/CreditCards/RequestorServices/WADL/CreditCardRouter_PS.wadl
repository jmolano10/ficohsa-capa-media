      <xsd:element name="getInterestRate_params">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="SourceBank" type="string"/>
            <xsd:element name="DestinationBank" type="string"/>
            <xsd:element name="ApplicationId" type="string"/>
            <xsd:element name="Language" type="string"/>
            <xsd:element name="Org" type="string"/>
            <xsd:element name="Logo" type="string"/>
            <xsd:element name="Pct" type="string"/>
          </xsd:sequence>
        </xsd:complexType>
