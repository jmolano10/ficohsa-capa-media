    <resource path="/customer">
      <method name="GET" soa:wsdlOperation="getCustomerCreditCard">
        <request soa:paramElement="pns:getCustomerCreditCardRequest"
                 xmlns:pns="http://www.ficohsa.com.hn/middleware.services/getCustomerCreditCardTypes">
          <param name="SourceBank" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:SourceBank"
                 type="xsd:string"/>
          <param name="DestinationBank" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:DestinationBank"
                 type="xsd:string"/>
          <param name="GlobalId" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:GlobalId"
                 type="xsd:string"/>
          <param name="ApplicationId" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:ApplicationId"
                 type="xsd:string"/>
          <param name="ApplicationUser" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:ApplicationUser"
                 type="xsd:string"/>
          <param name="BranchId" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:BranchId"
                 type="xsd:string"/>
          <param name="TransactionDate" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:TransactionDate"
                 type="xsd:string"/>
          <param name="Language" style="query" soa:expression="$msg.request/ns2:GeneralInfo/ns1:Language"
                 type="xsd:string"/>
          <param name="SearchCriteria" style="query" soa:expression="$msg.request/ns2:SearchCriteria"
                 type="xsd:string"/>
          <param name="SearchValue" style="query" soa:expression="$msg.request/ns2:SearchValue" type="xsd:string"/>
          <param name="Org" style="query" soa:expression="$msg.request/ns2:Org" type="xsd:string"/>
        </request>
        <response status="200">
          <representation mediaType="application/json" element="cns:getCustomerCreditCardResponse"
                          xmlns:cns="http://www.ficohsa.com.hn/middleware.services/getCustomerCreditCardTypes"/>
        </response>
      </method>
    </resource>
    <resource path="/activate-credit-card">
      <method name="POST" soa:wsdlOperation="activateCreditCard">
        <request>
          <representation mediaType="application/json" element="cns:activateCreditCardRequest"
