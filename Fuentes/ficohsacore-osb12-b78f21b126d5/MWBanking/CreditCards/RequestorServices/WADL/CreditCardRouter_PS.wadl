  <resources>
    <resource path="/financing-account">
      <method name="POST" soa:wsdlOperation="createFinancingAccount">
        <request>
          <representation mediaType="application/json" element="cns:createFinancingAccountRequest"
                          xmlns:cns="http://www.ficohsa.com.hn/middleware.services/creditCardsTypes"/>
        </request>
        <response status="200">
          <representation mediaType="application/json" element="cns:createFinancingAccountResponse"
                          xmlns:cns="http://www.ficohsa.com.hn/middleware.services/creditCardsTypes"/>
        </response>
      </method>
    </resource>
    <resource path="/financing-details">
      <method name="POST" soa:wsdlOperation="availableFinancingDetail">
