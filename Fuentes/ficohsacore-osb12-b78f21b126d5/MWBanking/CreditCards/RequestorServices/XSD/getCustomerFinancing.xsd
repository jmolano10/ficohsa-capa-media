<xsd:schema targetNamespace="http://www.ficohsa.com.hn/middleware.services/getCustomerFinancingTypes" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.example.org" xmlns:dtt="http://www.ficohsa.com.hn/middleware.services/commonTypes" xmlns:tns="http://www.ficohsa.com.hn/middleware.services/getCustomerFinancingTypes">
  <xsd:import schemaLocation="../../../../CommonResources/XSD/CommonTypes.xsd" namespace="http://www.ficohsa.com.hn/middleware.services/commonTypes"/>
  <xsd:simpleType name="stringMinLength1">
    <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CustomerFinancingInfoTypes">
    <xsd:sequence>
        <xsd:element name="Org" type="xsd:integer" minOccurs="0"/>
        <xsd:element name="Logo" type="xsd:integer" minOccurs="0"/>
        <xsd:element name="LoanTermType" type="xsd:integer" minOccurs="0"/>
        <xsd:element name="InterestRate" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="PrincipalAmount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="InterestAmount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="InsuranceAmount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="CommissionAmount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="LoanAmount" type="xsd:decimal" minOccurs="0"/>
