        <xsd:element name="MonthlyPayment" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="InsuranceAmtIncluded" type="xsd:string" minOccurs="0"/>
        <xsd:element name="LastPaymentAmount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="PctCode" type="xsd:string" minOccurs="0"/>
        <xsd:element name="InsuranceCode" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="getCustomerFinancingRequest">
      <xsd:annotation>
      <xsd:documentation>getCustomerFinancingRequest</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="GeneralInfo" type="dtt:GeneralInfoType" minOccurs="0"/>
        <xsd:element name="Org" type="xsd:integer" minOccurs="0"/>
        <xsd:element name="Amount" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="InterestRate" type="xsd:decimal" minOccurs="0"/>
        <xsd:element name="LoanTerm" type="xsd:integer" minOccurs="0"/>
        <xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
        <xsd:element name="PctCode" type="xsd:string" minOccurs="0"/>
        <xsd:element name="InsuranceCode" type="xsd:string" minOccurs="0"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getCustomerFinancingResponse">
    <xsd:annotation>
      <xsd:documentation>getCustomerFinancingResponse</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="StatusInfo" type="dtt:StatusInfoType" minOccurs="0"/>
        <xsd:element name="FinancingDetailsInfo" type="tns:CustomerFinancingInfoTypes" minOccurs="0"/>
        <xsd:element name="ErrorInfo" minOccurs="0" type="dtt:ErrorInfoType"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>