        <xsd:schema>
            <xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/commonTypes" schemaLocation="../../../../../../CommonResources/XSD/CommonTypes.xsd"/>
        </xsd:schema>
  </types>
  
    <!-- DoEcho Messages -->
    <message name="doEchoRequest">
        <part element="inp1:DoEchoRequest" name="parameters"/>
        <part element="dtt:RequestHeader" name="RequestHeader"/>
    </message>
    <message name="doEchoResponse">
        <part element="inp1:DoEchoResponse" name="parameters"/>
        <part element="dtt:ResponseHeader" name="ResponseHeader"/>
    </message>
  
    <!-- GetTicket Messages -->
    <message name="getTicketRequest">
        <part element="inp1:GetTicketRequest" name="parameters"/>
