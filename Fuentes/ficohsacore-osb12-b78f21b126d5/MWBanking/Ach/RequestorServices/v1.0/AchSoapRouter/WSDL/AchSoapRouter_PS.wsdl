        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="DoEcho">
            <soap:operation style="document" soapAction="urn:AchSoapRouter_PS/DoEcho"/>
            <input>
                <soap:body use="literal" parts="parameters"/>
                <soap:header message="tns:doEchoRequest" use="literal" part="RequestHeader"/>
