  <portType name="AchSoapRouter_PS_ptt">
    <operation name="DoEcho">
      <input message="tns:doEchoRequest"/>
      <output message="tns:doEchoResponse"/>
    </operation>
    <operation name="GetTicket">
      <input message="tns:getTicketRequest"/>
      <output message="tns:getTicketResponse"/>
    </operation>
    <operation name="SendCredit">
      <input message="tns:sendCreditRequest"/>
      <output message="tns:sendCreditResponse"/>
    </operation>
    <operation name="GetTransactionsStatus">
      <input message="tns:getTransactionsStatusRequest"/>
      <output message="tns:getTransactionsStatusResponse"/>
    </operation>
    <operation name="GetReportTransaction">
      <input message="tns:getReportTransactionRequest"/>
      <output message="tns:getReportTransactionResponse"/>
    </operation>
  </portType>
