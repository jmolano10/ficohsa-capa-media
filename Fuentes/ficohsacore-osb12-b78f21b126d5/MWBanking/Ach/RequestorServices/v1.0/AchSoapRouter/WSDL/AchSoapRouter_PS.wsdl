    </message>
    <message name="getTicketResponse">
        <part element="inp1:GetTicketResponse" name="parameters"/>
        <part element="dtt:ResponseHeader" name="ResponseHeader"/>
    </message>
  
    <!-- SendCredit Messages -->
    <message name="sendCreditRequest">
        <part element="inp1:SendCreditRequest" name="parameters"/>
        <part element="dtt:RequestHeader" name="RequestHeader"/>
    </message>
    <message name="sendCreditResponse">
        <part element="inp1:SendCreditResponse" name="parameters"/>
