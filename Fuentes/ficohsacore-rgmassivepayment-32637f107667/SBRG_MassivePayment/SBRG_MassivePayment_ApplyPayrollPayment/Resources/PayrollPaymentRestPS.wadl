<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="https://www.ficohsa.com/regional/massivePayment/payrollPaymentTypes" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="PayrollPaymentRestPS">RestService</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="https://www.ficohsa.com/regional/massivePayment/payrollPaymentTypes" schemaLocation="../../SBRG_MassivePayment_Commons/Schemas/PayrollPaymentTypes.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/">
         <method name="POST" soa:wsdlOperation="PayrollPayment">
            <request>
               <representation mediaType="application/json" element="cns:PayrollPayment" xmlns:cns="https://www.ficohsa.com/regional/massivePayment/payrollPaymentTypes"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:PayrollPaymentResponse" xmlns:cns="https://www.ficohsa.com/regional/massivePayment/payrollPaymentTypes"/>
            </response>
         </method>
      </resource>
   </resources>
</application>