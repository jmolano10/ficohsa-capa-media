<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="LoggingRegionalRestBS">RestService</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="https://www.ficohsa.com/regional/utility/logging" schemaLocation="SBRG_Logging_Commons/Schemas/LoggingRegional.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/">
         <method name="POST" soa:wsdlOperation="SaveLogInFileSystem">
            <request>
               <representation mediaType="application/xml" element="cns:LoggingInput" xmlns:cns="https://www.ficohsa.com/regional/utility/logging"/>
               <representation mediaType="application/json" element="cns:LoggingInput" xmlns:cns="https://www.ficohsa.com/regional/utility/logging"/>
            </request>
            <response status="202"/>
         </method>
      </resource>
   </resources>
</application>