<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://wadl.dev.java.net/2009/02">
  <doc title="AuthorizeBatchRestPS">RestService</doc>
  <grammars>
    <xsd:schema>
      <xsd:import namespace="https://www.ficohsa.com/regional/massivePayment" schemaLocation="../Schemas/AuthorizeBatchTypes.xsd"/>
    </xsd:schema>
  </grammars>
  <resources>
    <resource path="/">
      <method name="POST" soa:wsdlOperation="authorizeBatch">
        <request>
          <representation mediaType="application/json" element="cns:authorizeBatch" xmlns:cns="https://www.ficohsa.com/regional/massivePayment"/>
        </request>
        <response status="200">
          <representation mediaType="application/json" element="cns:authorizeBatchResponse" xmlns:cns="https://www.ficohsa.com/regional/massivePayment"/>
        </response>
      </method>
    </resource>
  </resources>
</application>