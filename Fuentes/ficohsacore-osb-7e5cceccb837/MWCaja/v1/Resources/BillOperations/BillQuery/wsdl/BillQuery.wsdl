<wsdl:definitions name="BillQueryPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/BillQueryPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:tns="http://www.ficohsa.com.hn/middleware.services/BillQueryPS/"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:bil="http://www.ficohsa.com.hn/middleware.services/billOperationsTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/billOperationsTypes"
				schemaLocation="../../xsd/BillOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>


	<wsdl:message name="billQueryRequest">
		<wsdl:part element="bil:billQuery" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="billQueryResponse">
		<wsdl:part element="bil:billQueryResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>


	<wsdl:portType name="BillQueryPS">
		<wsdl:operation name="billQuery"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="tns:billQueryRequest" />
			<wsdl:output message="tns:billQueryResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="BillQueryPSSOAP" type="tns:BillQueryPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="billQuery">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/BillQueryPS/billQuery" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="tns:billQueryRequest" part="RequestHeader"
					use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="tns:billQueryResponse" part="ResponseHeader"
					use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="BillQueryPS">
		<wsdl:port binding="tns:BillQueryPSSOAP" name="BillQueryPSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/agreements/billOperations" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>