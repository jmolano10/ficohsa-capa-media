<wsdl:definitions name="ReversalRemitPaymentPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/ReversalRemitPaymentPS/"
	xmlns:irp="http://www.ficohsa.com.hn/middleware.services/ReversalRemitPaymentPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:mon="http://www.ficohsa.com.hn/middleware.services/moneyTransferOperationsTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/moneyTransferOperationsTypes"
				schemaLocation="../../xsd/MoneyTransferOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="ReversalRemitPaymentRequest">
		<wsdl:part element="mon:reversalRemitPayment" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="ReversalRemitPaymentResponse">
		<wsdl:part element="mon:reversalRemitPaymentResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>
	<wsdl:portType name="ReversalRemitPaymentPS">
		<wsdl:operation name="ReversalRemitPayment"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="irp:ReversalRemitPaymentRequest" />
			<wsdl:output message="irp:ReversalRemitPaymentResponse" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="ReversalRemitPaymentPSSOAP" type="irp:ReversalRemitPaymentPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="ReversalRemitPayment">
			<soap:operation
				soapAction="http://www.example.org/ReversalRemitPaymentPS/ReversalRemitPayment" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="irp:ReversalRemitPaymentRequest"
					part="RequestHeader" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="irp:ReversalRemitPaymentResponse"
					part="ResponseHeader" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="ReversalRemitPaymentPS">
		<wsdl:port binding="irp:ReversalRemitPaymentPSSOAP" name="ReversalRemitPaymentPSSOAP">
			<soap:address
				location="http://mwteller:8004/mwteller/moneyTransfer/moneyTransferOperations" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>