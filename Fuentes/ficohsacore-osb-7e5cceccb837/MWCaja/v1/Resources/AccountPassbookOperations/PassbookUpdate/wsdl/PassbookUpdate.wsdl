<wsdl:definitions 
	name="PassbookUpdatePS"
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/PassbookUpdatePS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType" 
	xmlns:apo="http://www.ficohsa.com.hn/middleware.services/accountPassbookOperationsTypes"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/PassbookUpdatePS/">
	
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType" 
				schemaLocation="../../../General/xsd/Header.xsd"/>
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/accountPassbookOperationsTypes"
				schemaLocation="../../xsd/AccountPassbookOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="passbookUpdateRequest">
		<wsdl:part element="apo:passbookUpdate" name="parameters"/>
		<wsdl:part name="RequestHeader" element="auth:RequestHeader"/>
	</wsdl:message>
	<wsdl:message name="passbookUpdateResponse">
		<wsdl:part element="apo:passbookUpdateResponse" name="result"/>
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader"/>
	</wsdl:message>
	
	<wsdl:portType name="PassbookUpdatePS">
		<wsdl:operation name="passbookUpdate" parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:passbookUpdateRequest"/>
			<wsdl:output message="fic:passbookUpdateResponse"/>
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="PassbookUpdatePSSOAP" type="fic:PassbookUpdatePS">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="passbookUpdate">
			<soap:operation soapAction="http://www.ficohsa.com.hn/middleware.services/PassbookUpdatePS/passbookUpdate"/>
			<wsdl:input>
				<soap:body use="literal" parts="parameters"/>
				<soap:header message="fic:passbookUpdateRequest" part="RequestHeader" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result"/>
				<soap:header message="fic:passbookUpdateResponse" part="ResponseHeader" use="literal"/>
			</wsdl:output>
		</wsdl:operation>	
	</wsdl:binding>
	
	<wsdl:service name="PassbookUpdatePS">
		<wsdl:port binding="fic:PassbookUpdatePSSOAP" name="PassbookUpdatePSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/accounts/accountPassbookOperations" />
		</wsdl:port>
	</wsdl:service>
	
</wsdl:definitions>