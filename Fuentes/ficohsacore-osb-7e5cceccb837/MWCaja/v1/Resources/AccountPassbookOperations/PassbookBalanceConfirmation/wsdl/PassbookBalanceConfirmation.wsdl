<wsdl:definitions 
	name="PassbookBalanceConfirmationPS" 
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/PassbookBalanceConfirmationPS/" 
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/PassbookBalanceConfirmationPS/" 
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType" 
	xmlns:apo="http://www.ficohsa.com.hn/middleware.services/accountPassbookOperationsTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType" 
				schemaLocation="../../../General/xsd/Header.xsd"/>
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/accountPassbookOperationsTypes" 
				schemaLocation="../../xsd/AccountPassbookOperationsTypes.xsd"/>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="passbookBalanceConfirmationRequest">
		<wsdl:part element="apo:passbookBalanceConfirmation" name="parameters"/>
		<wsdl:part name="RequestHeader" element="auth:RequestHeader"/>
	</wsdl:message>
	<wsdl:message name="passbookBalanceConfirmationResponse">
		<wsdl:part element="apo:passbookBalanceConfirmationResponse" name="result"/>
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader"/>
	</wsdl:message>

	<wsdl:portType name="PassbookBalanceConfirmationPS">
		<wsdl:operation name="passbookBalanceConfirmation" parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:passbookBalanceConfirmationRequest"/>
			<wsdl:output message="fic:passbookBalanceConfirmationResponse"/>
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="PassbookBalanceConfirmationPSSOAP" type="fic:PassbookBalanceConfirmationPS">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="passbookBalanceConfirmation">
			<soap:operation soapAction="http://www.ficohsa.com.hn/middleware.services/PassbookBalanceConfirmationPS/passbookBalanceConfirmation"/>
			<wsdl:input>
				<soap:body use="literal" parts="parameters"/>
				<soap:header message="fic:passbookBalanceConfirmationRequest" part="RequestHeader" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result"/>
				<soap:header message="fic:passbookBalanceConfirmationResponse" part="ResponseHeader" use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="PassbookBalanceConfirmationPS">
		<wsdl:port binding="fic:PassbookBalanceConfirmationPSSOAP" name="PassbookBalanceConfirmationPSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/accounts/accountPassbookOperations"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>