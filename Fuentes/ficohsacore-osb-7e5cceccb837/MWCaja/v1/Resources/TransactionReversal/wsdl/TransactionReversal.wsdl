<wsdl:definitions name="transactionReversalPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/transactionReversalPS/"
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/transactionReversalPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:txr="http://www.ficohsa.com.hn/middleware.services/transactionReversalTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/transactionReversalTypes"
				schemaLocation="../xsd/TransactionReversalTypes.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="transactionReversalRequest">
		<wsdl:part element="txr:transactionReversal" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="transactionReversalResponse">
		<wsdl:part element="txr:transactionReversalResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>

	<wsdl:portType name="transactionReversalPS">
		<wsdl:operation name="transactionReversal"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:transactionReversalRequest" />
			<wsdl:output message="fic:transactionReversalResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="transactionReversalPSSOAP" type="fic:transactionReversalPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="transactionReversal">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/transactionReversalPS/transactionReversal" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:transactionReversalRequest"
					part="RequestHeader" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:transactionReversalResponse"
					part="ResponseHeader" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="transactionReversalPS">
		<wsdl:port binding="fic:transactionReversalPSSOAP" name="transactionReversalPSSOAP">
			<soap:address
				location="http://mwteller:8004/mwteller/branch/transactionReversal" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>