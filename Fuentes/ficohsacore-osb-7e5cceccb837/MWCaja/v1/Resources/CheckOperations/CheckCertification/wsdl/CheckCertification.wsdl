<wsdl:definitions 
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/CheckCertificationPS/" 
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	name="CheckCertificationPS" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:cko="http://www.ficohsa.com.hn/middleware.services/checkOperationsTypes"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/CheckCertificationPS/">
  <wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/checkOperationsTypes"
				schemaLocation="../../xsd/CheckOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
  <wsdl:message name="checkCertificationRequest">
		<wsdl:part element="cko:checkCertification" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="checkCertificationResponse">
		<wsdl:part element="cko:checkCertificationResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>
  <wsdl:portType name="CheckCertificationPS">
    <wsdl:operation name="checkCertification"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:checkCertificationRequest" />
			<wsdl:output message="fic:checkCertificationResponse" />
	</wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="CheckCertificationPSSOAP" type="fic:CheckCertificationPS">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="checkCertification">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/CheckCertificationPS/checkCertification" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:checkCertificationRequest"
					part="RequestHeader" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:checkCertificationResponse"
					part="ResponseHeader" use="literal" />
			</wsdl:output>
		</wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="CheckCertificationPS">
    <wsdl:port binding="fic:CheckCertificationPSSOAP" name="CheckCertificationPSSOAP">
      <soap:address location="http://www.example.org/"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
