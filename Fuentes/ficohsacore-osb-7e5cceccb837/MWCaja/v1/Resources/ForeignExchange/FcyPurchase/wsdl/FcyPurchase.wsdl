<wsdl:definitions name="fcyPurchasePS" targetNamespace="http://www.ficohsa.com.hn/middleware.services/fcyPurchasePS/" xmlns:fic="http://www.ficohsa.com.hn/middleware.services/fcyPurchasePS/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType" xmlns:fox="http://www.ficohsa.com.hn/middleware.services/foreignExchangeTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType" schemaLocation="../../../General/xsd/Header.xsd"/>
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/foreignExchangeTypes" schemaLocation="../../xsd/ForeignExchangeTypes.xsd"/>
		</xsd:schema>
	</wsdl:types>

    <wsdl:message name="fcyPurchaseRequest">
		<wsdl:part element="fox:fcyPurchase" name="parameters"/>
		<wsdl:part name="RequestHeader" element="auth:RequestHeader"/>
	</wsdl:message>
	<wsdl:message name="fcyPurchaseResponse">
		<wsdl:part element="fox:fcyPurchaseResponse" name="result"/>
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader"/>
	</wsdl:message>

	<wsdl:portType name="fcyPurchasePS">
		<wsdl:operation name="fcyPurchase" parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:fcyPurchaseRequest"/>
			<wsdl:output message="fic:fcyPurchaseResponse"/>
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="fcyPurchasePSSOAP" type="fic:fcyPurchasePS">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="fcyPurchase">
			<soap:operation soapAction="http://www.ficohsa.com.hn/middleware.services/fcyPurchasePS/fcyPurchase"/>
			<wsdl:input>
				<soap:body use="literal" parts="parameters"/>
				<soap:header message="fic:fcyPurchaseRequest" part="RequestHeader" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result"/>
				<soap:header message="fic:fcyPurchaseResponse" part="ResponseHeader" use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="fcyPurchasePS">
		<wsdl:port binding="fic:fcyPurchasePSSOAP" name="fcyPurchasePSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/forex/foreignExchange"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>