<wsdl:definitions 
	name="DirectForexQueryPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/DirectForexQueryPS/"
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/DirectForexQueryPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:fox="http://www.ficohsa.com.hn/middleware.services/foreignExchangeTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/foreignExchangeTypes"
				schemaLocation="../../xsd/ForeignExchangeTypes.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="directForexQueryRequest">
		<wsdl:part element="fox:directForexQuery" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="directForexQueryResponse">
		<wsdl:part element="fox:directForexQueryResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>

	<wsdl:portType name="DirectForexQueryPS">
		<wsdl:operation name="directForexQuery"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:directForexQueryRequest" />
			<wsdl:output message="fic:directForexQueryResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="DirectForexQueryPSSOAP" type="fic:DirectForexQueryPS">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="directForexQuery">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/DirectForexQueryPS/directForexQuery" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:directForexQueryRequest" part="RequestHeader"
					use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:directForexQueryResponse" part="ResponseHeader"
					use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="DirectForexQueryPS">
		<wsdl:port binding="fic:DirectForexQueryPSSOAP" name="DirectForexQueryPSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/forex/foreignExchange" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>