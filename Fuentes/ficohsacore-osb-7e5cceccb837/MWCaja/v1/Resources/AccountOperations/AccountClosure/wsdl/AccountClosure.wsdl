<wsdl:definitions 
	name="AccountClosurePS"
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/AccountClosurePS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:aco="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/AccountClosurePS/">
	
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
				schemaLocation="../../xsd/AccountOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="accountClosureRequest">
		<wsdl:part element="aco:accountClosure" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	
	<wsdl:message name="accountClosureResponse">
		<wsdl:part element="aco:accountClosureResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>
	
	<wsdl:portType name="AccountClosurePS">
		<wsdl:operation name="accountClosure"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:accountClosureRequest" />
			<wsdl:output message="fic:accountClosureResponse" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="AccountClosurePSSOAP" type="fic:AccountClosurePS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="accountClosure">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/AccountClosurePS/accountClosure" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:accountClosureRequest" part="RequestHeader"
					use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:accountClosureResponse" part="ResponseHeader"
					use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	
	<wsdl:service name="AccountClosurePS">
		<wsdl:port binding="fic:AccountClosurePSSOAP" name="AccountClosurePSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/accounts/accountOperations" />
		</wsdl:port>
	</wsdl:service>
	
</wsdl:definitions>