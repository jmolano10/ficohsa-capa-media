<wsdl:definitions name="AccountDepositPS" targetNamespace="http://www.ficohsa.com.hn/middleware.services/AccountDepositPS/" xmlns:fic="http://www.ficohsa.com.hn/middleware.services/AccountDepositPS/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType" xmlns:aco="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes">
	
