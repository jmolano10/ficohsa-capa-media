<wsdl:definitions 
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/AccountQueryPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	name="AccountQueryPS"
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:aco="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/AccountQueryPS/">
	
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
				schemaLocation="../../xsd/AccountOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="accountQueryRequest">
		<wsdl:part element="aco:accountQuery" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="accountQueryResponse">
		<wsdl:part element="aco:accountQueryResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>
	
	<wsdl:portType name="AccountQueryPS">
		<wsdl:operation name="accountQuery"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:accountQueryRequest" />
			<wsdl:output message="fic:accountQueryResponse" />
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="AccountQueryPSSOAP" type="fic:AccountQueryPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="accountQuery">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/AccountQueryPS/accountQuery" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:accountQueryRequest" part="RequestHeader"
					use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:accountQueryResponse" part="ResponseHeader"
					use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="AccountQueryPS">
		<wsdl:port binding="fic:AccountQueryPSSOAP" name="AccountQueryPSSOAP">
			<soap:address location="http://mwteller:8004/mwteller/accounts/accountOperations" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
