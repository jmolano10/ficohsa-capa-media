<wsdl:definitions 
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/AccountActivationPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:aco="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
	name="AccountActivationPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/AccountActivationPS/">
	
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../../General/xsd/Header.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/AccountOperationsTypes"
				schemaLocation="../../xsd/AccountOperationsTypes.xsd" />
		</xsd:schema>
	</wsdl:types>
	
	<wsdl:message name="accountActivationRequest">
		<wsdl:part element="aco:accountActivation" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="accountActivationResponse">
		<wsdl:part element="aco:accountActivationResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>
	
	<wsdl:portType name="AccountActivationPS">
		<wsdl:operation name="accountActivation"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:accountActivationRequest" />
			<wsdl:output message="fic:accountActivationResponse" />
		</wsdl:operation>
	</wsdl:portType>
	
	<wsdl:binding name="AccountActivationPSSOAP" type="fic:AccountActivationPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="accountActivation">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/AccountActivationPS/accountActivation" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:accountActivationRequest"
					part="RequestHeader" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:accountActivationResponse"
					part="ResponseHeader" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="AccountActivationPS">
		<wsdl:port binding="fic:AccountActivationPSSOAP" name="AccountActivationPSSOAP">
			<soap:address location="http://www.example.org/" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
