<wsdl:definitions name="loginPS" targetNamespace="http://www.ficohsa.com.hn/middleware.services/loginPS/" xmlns:fic="http://www.ficohsa.com.hn/middleware.services/loginPS/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType" xmlns:cons="http://www.ficohsa.com.hn/middleware.services/loginTypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType" schemaLocation="../../esquemas_generales/HeaderElements.xsd"/>
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/loginTypes" schemaLocation="../xsd/loginTypes.xsd"/>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="loginRequest">
		<wsdl:part element="cons:login" name="parameters"/>
		<wsdl:part name="RequestHeader" element="auth:RequestHeader"/>
	</wsdl:message>
	<wsdl:message name="loginResponse">
		<wsdl:part element="cons:loginResponse" name="result"/>
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader"/>
	</wsdl:message>

	<wsdl:portType name="loginPS">
		<wsdl:operation name="login" parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:loginRequest"/>
			<wsdl:output message="fic:loginResponse"/>
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="loginPSSOAP" type="fic:loginPS">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="login">
			<soap:operation soapAction="http://www.ficohsa.com.hn/middleware.services/loginPS/login"/>
			<wsdl:input>
				<soap:body use="literal" parts="parameters"/>
				<soap:header message="fic:loginRequest" part="RequestHeader" use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result"/>
				<soap:header message="fic:loginResponse" part="ResponseHeader" use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="loginPS">
		<wsdl:port binding="fic:loginPSSOAP" name="loginPSSOAP">
			<soap:address location="https://172.23.13.19:8004//Middleware/SalesAndServices/PartyAuthentication/Login"/>
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>