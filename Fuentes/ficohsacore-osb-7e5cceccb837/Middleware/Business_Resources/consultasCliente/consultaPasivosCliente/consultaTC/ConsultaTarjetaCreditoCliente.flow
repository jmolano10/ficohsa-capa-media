<bpel:process name="ConsultaTarjetaCreditoCliente" targetNamespace="http://www.example.com/flow/Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/ConsultaTarjetaCreditoCliente" bea:name="ConsultaTarjetaCreditoCliente" xmlns:tns="http://www.example.com/flow/Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/ConsultaTarjetaCreditoCliente" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:rescon="http://www.bea.com/alsb/flow/resources/config" xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions" xmlns:expr="http://www.bea.com/wli/sb/stages/config" xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config" xmlns:bind="ConsultaTarjetaCreditoCliente" xmlns:ns1="T24WebServicesImpl" xmlns:ns0="http://www.ficohsa.com.hn/middleware.services/consultaSaldosTarjetaCreditoPS/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <bpel:partnerLinks>
        <bpel:partnerLink name="ConsultaTarjetaCreditoCliente" partnerLinkType="tns:ConsultaTarjetaCreditoCliente" myRole="ConsultaTarjetaCreditoCliente">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="consultasClienteBS" partnerLinkType="tns:consultasClienteBS_plnkType" partnerRole="consultasClienteBS_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="ConsultaSaldosTarjetaCredito" partnerLinkType="tns:ConsultaSaldosTarjetaCredito_plnkType" partnerRole="ConsultaSaldosTarjetaCredito_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request" messageType="bind:ConsultaTarjetaCreditoClienteRequest">
	       </bpel:variable>
	       <bpel:variable name="response" messageType="bind:ConsultaTarjetaCreditoClienteResponse">
	       </bpel:variable>
    <bpel:variable name="REQTarjetasCreditoPorCliente" messageType="ns1:Tarjetasdecreditoporcliente">
    </bpel:variable>
    <bpel:variable name="RSPTarjetasCreditoPorCliente" messageType="ns1:TarjetasdecreditoporclienteResponse">
    </bpel:variable>
    
    
    <bpel:variable name="cardCounter" type="xsd:string"/></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="ConsultaTarjetaCreditoCliente" operation="ConsultaTarjetaCreditoCliente" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/ConsultaTarjetaCreditoCliente" binding="bind:ConsultaTarjetaCreditoClienteBinding"/>
            </rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="REQTarjetasCreditoPorCliente.parameters">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/consultaTarjetasCreditoClienteIn"/>
        					<expr:param name="consultaTarjetaCreditoTypeRequest1">
        						<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:invoke operation="Tarjetasdecreditoporcliente" partnerLink="consultasClienteBS" inputVariable="REQTarjetasCreditoPorCliente" outputVariable="RSPTarjetasCreditoPorCliente">
        	<rescon:invokeInfo>
        		<rescon:service isProxy="false" ref="Middleware/Business_Resources/consultasCliente/Resources/consultasClienteBS"/></rescon:invokeInfo></bpel:invoke>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.outputParameters">
        			<trf:expr>
        				<expr:xqueryText>&lt;bind:ConsultaTarjetaCreditoTypeResponse>
	&lt;bind:CREDIT_CARD_DETAILS>
	&lt;/bind:CREDIT_CARD_DETAILS>
&lt;/bind:ConsultaTarjetaCreditoTypeResponse></expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:forEach parallel="yes" counterName="counter">
        	<bpel:startCounterValue>number(1)</bpel:startCounterValue>
        	<bpel:finalCounterValue>count($RSPTarjetasCreditoPorCliente.parameters/FICOLISTCLIENTCARDCPType/gFICOLISTCLIENTCARDCPDetailType/mFICOLISTCLIENTCARDCPDetailType[STATUS!="29 - TARJETA DADA DE BAJA" and CATEGORY!="Additional"]/CARDNO)</bpel:finalCounterValue>
        	<bpel:scope>
        		
        		
        		<bpel:variables>
        			<bpel:variable name="REQConsultarSaldosTCLocal" messageType="ns0:consultaSaldosTarjetaCreditoRequest">
        			</bpel:variable>
        			<bpel:variable name="RSPConsultarSaldosTCLocal" messageType="ns0:consultaSaldosTarjetaCreditoResponse">
        			</bpel:variable></bpel:variables>
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="cardCounter">
        						<trf:expr>
        							<expr:xqueryText>$counter</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="REQConsultarSaldosTCLocal.AutenticacionRequestHeader">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/consultaSaldosTCHeaderIn"/>
        								<expr:param name="consultaTarjetaCreditoTypeRequest1">
        									<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="REQConsultarSaldosTCLocal.parameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/consultaSaldosTCBodyIn"/>
        								<expr:param name="counter">
        									<expr:path>$counter</expr:path></expr:param>
        								<expr:param name="tarjetasdecreditoporclienteResponse1">
        									<expr:path>$RSPTarjetasCreditoPorCliente.parameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        			<bpel:invoke partnerLink="ConsultaSaldosTarjetaCredito" operation="consultaSaldosTarjetaCredito" inputVariable="REQConsultarSaldosTCLocal" outputVariable="RSPConsultarSaldosTCLocal">
        				<rescon:invokeInfo>
        					<rescon:service isProxy="true" ref="Middleware/OperationsAndExecution/Cards/ConsultaSaldosTarjetaCredito"/></rescon:invokeInfo></bpel:invoke>
        			<bpel:assign>
        				
        				<bpel:extensionAssignOperation>
        					<trf:insert varName="response.outputParameters">
        						<trf:location>
        							<expr:xpathText>./bind:CREDIT_CARD_DETAILS</expr:xpathText></trf:location>
        						<trf:where>last-child</trf:where>
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/consultaSaldosTCClienteOut"/>
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								
        								<expr:param name="consultaSaldosTarjetaCreditoResponse1">
        									<expr:path>$RSPConsultarSaldosTCLocal.result</expr:path></expr:param>
        								<expr:param name="mFICOLISTCLIENTCARDCPDetailType1">
        									<expr:path>$RSPTarjetasCreditoPorCliente.parameters/FICOLISTCLIENTCARDCPType/gFICOLISTCLIENTCARDCPDetailType/mFICOLISTCLIENTCARDCPDetailType[CARDNO=fn:string($RSPConsultarSaldosTCLocal.result/CARD_NUMBER/text())]</expr:path></expr:param>
        								<expr:param name="successIndicator">
        									<expr:path>fn:string($RSPConsultarSaldosTCLocal.ResponseHeader/successIndicator/text())</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign>
        			
        			
        			
        			
        			</bpel:sequence>
        	</bpel:scope>
        </bpel:forEach>
        <bpel:if>
        	<bpel:condition>count($response.outputParameters/bind:CREDIT_CARD_DETAILS/bind:CREDIT_CARD_DETAIL) > 0</bpel:condition>
        	<bpel:sequence>
        		<bpel:empty/>
        	<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation/>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>SUCCESS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert>
		</bpel:extensionAssignOperation>
        	</bpel:assign></bpel:sequence>
        	<bpel:else>
        		<bpel:sequence>
        			<bpel:empty/>
        		<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation/>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>NO RECORDS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        	</bpel:else>
        </bpel:if>
        <bpel:reply partnerLink="ConsultaTarjetaCreditoCliente" operation="ConsultaTarjetaCreditoCliente" variable="response"/>
    </bpel:sequence>
</bpel:process>