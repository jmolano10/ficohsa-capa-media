	            <xs:element name="REF_CURRENCY" type="xs:string" />
	            <xs:element name="REF_EXCHANGE_RATE" type="xs:string" />
	            <xs:element name="REF_AMOUNT" type="xs:string" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="pagoRemesaCreditoCuentaResponse" type="cons:pagoRemesaCreditoCuentaResponseType" />
	
</xs:schema>