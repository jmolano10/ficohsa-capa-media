<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	version="1.0" targetNamespace="http://www.ficohsa.com.hn/middleware.services/consultaPagoTarjetaCreditoTypes"
	xmlns:cred="http://www.ficohsa.com.hn/middleware.services/consultaPagoTarjetaCreditoTypes">
	
	<xs:complexType name="consultaPagoTarjetaCreditoResponseType">
		<xs:sequence>
			<xs:element name="CONFIRMATION_STATUS" type="xs:string" minOccurs="0"/>
			<xs:element name="CONFIRMATION_MESSAGE" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="consultaPagoTarjetaCredito">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PAYMENT_REFERENCE" type="xs:string"/>
				<xs:element name="CREDIT_CARD_NUMBER" type="xs:string"/>
				<xs:element name="DATE" type="xs:string" />
				<xs:element name="AMOUNT" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>	
	<xs:element name="consultaPagoTarjetaCreditoResponse" type="cred:consultaPagoTarjetaCreditoResponseType"/>
	
</xs:schema>