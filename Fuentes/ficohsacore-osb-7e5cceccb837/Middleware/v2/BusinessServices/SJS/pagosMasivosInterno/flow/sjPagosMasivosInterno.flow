        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:assign>
		
		<bpel:extensionAssignOperation>
			<trf:assign varName="validarSaldo">
				<trf:expr>
					<expr:xqueryText>fn:string($request.inputParameters/BATCHES/BATCH[xs:int($contador)]/DATA_STRUCTURE/VALIDATIONS/DEBIT_ACCT_BALANCE/text()) = &quot;YES&quot;</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
		<bpel:extensionAssignOperation>
			<trf:assign varName="validarEstado">
				<trf:expr>
					<expr:xqueryText>fn:string($request.inputParameters/BATCHES/BATCH[xs:int($contador)]/DATA_STRUCTURE/VALIDATIONS/DEBIT_ACCT_STATUS/text()) = &quot;YES&quot;</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        				<bpel:if>
        					<bpel:condition>$validarSaldo or $validarEstado</bpel:condition>
        					<bpel:sequence>
        						<bpel:empty></bpel:empty>
        					<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation></bpel:documentation>
			<trf:assign varName="batchAgrupado">
				<trf:expr>
					<expr:xqueryTransform>
						<expr:resource
							ref="Middleware/v2/Resources/PagosMasivosInterno/xq/agrupaTxnLoteXCuenta"></expr:resource>
						<expr:param name="batch">
							<expr:path>$request.inputParameters/BATCHES/BATCH[xs:int($contador)]</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        					<bpel:else>
        						<bpel:sequence>
        							<bpel:empty></bpel:empty>
        						</bpel:sequence>
        					</bpel:else>
        				</bpel:if>
        				<bpel:if>
        					<bpel:condition>$validarSaldo or $validarEstado</bpel:condition>
        					<bpel:sequence>
        						<bpel:empty></bpel:empty>
        					
        					<bpel:forEach parallel="no"
        						counterName="contadorCuentas">
        						<bpel:startCounterValue>1</bpel:startCounterValue>
        						<bpel:finalCounterValue>count($batchAgrupado/ACCOUNT)</bpel:finalCounterValue>
        						<bpel:scope>
        							<bpel:sequence>
        								<bpel:if>
        									<bpel:condition>$validarSaldo</bpel:condition>
        									<bpel:sequence>
        										<bpel:empty></bpel:empty>
        									<bpel:scope>
		<bpel:sequence>
			<bpel:assign>
				<bpel:extensionAssignOperation>
					<bpel:documentation></bpel:documentation>
					<trf:assign
						varName="REQconsultaDetallesCuentaMasivos.parameters">
						<trf:expr>
							<expr:xqueryTransform>
								<expr:resource
									ref="Middleware/v2/BusinessServices/SJS/pagosMasivosInterno/xq/consultaDetallesCuentaMasivosIn"></expr:resource>
								
								<expr:param name="accountNumber">
									<expr:path>data($batchAgrupado/ACCOUNT[xs:int($contadorCuentas)]/ACCOUNT_NUMBER)</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
			<bpel:invoke
				partnerLink="ConsultaDetallesCuentaMasivos" operation="consultaDetallesCuentaMasivos" inputVariable="REQconsultaDetallesCuentaMasivos" outputVariable="RSPconsultaDetallesCuentaMasivos">
				<rescon:invokeInfo>
					<rescon:service
						ref="Middleware/v2/ProxyServices/ConsultaDetallesCuentaMasivos"
						isProxy="true"></rescon:service></rescon:invokeInfo></bpel:invoke>
			<bpel:assign>
				<bpel:extensionAssignOperation>
					<trf:assign varName="disponible">
						<trf:expr>
							<expr:xqueryText>let $saldoDisp := fn:string($RSPconsultaDetallesCuentaMasivos.result/CURRENT_BALANCE/text())
return
	if($saldoDisp = &quot;&quot;) then (
		0
	) else (
		xs:decimal($saldoDisp)
	)</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
				<bpel:extensionAssignOperation>
					<trf:assign varName="totalAmount">
						<trf:expr>
							<expr:xqueryText>data($batchAgrupado/ACCOUNT[xs:int($contadorCuentas)]/TOTAL_AMOUNT)</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
			<bpel:if>
				<bpel:condition>$disponible &lt; $totalAmount</bpel:condition>
				<bpel:sequence>
					<bpel:empty></bpel:empty>
					<bpel:assign>
						<bpel:extensionAssignOperation>
							<bpel:documentation></bpel:documentation>
							<trf:replace varName="batchError"
								contents-only="true">
								<trf:expr>
									<expr:xqueryText>&lt;BATCH&gt;
	&lt;ID_POSITION&gt;{$contador}&lt;/ID_POSITION&gt;
	&lt;CUSTOMER_BATCH_ID&gt;{fn:string($request.inputParameters/BATCHES/BATCH[xs:int($contador)]/CUSTOMER_BATCH_ID/text())}&lt;/CUSTOMER_BATCH_ID&gt;
	&lt;STATUS&gt;ERROR&lt;/STATUS&gt;
	&lt;ERROR_CODE&gt;ERROR&lt;/ERROR_CODE&gt;
	&lt;ERROR_MESSAGE&gt;{fn:concat(&quot;La cuenta &quot;,data($batchAgrupado/ACCOUNT[xs:int($contadorCuentas)]/ACCOUNT_NUMBER), &quot; no tiene saldo suficiente&quot;)}&lt;/ERROR_MESSAGE&gt;
&lt;/BATCH&gt;</expr:xqueryText></trf:expr></trf:replace></bpel:extensionAssignOperation></bpel:assign>
					<bpel:throw faultName="tns:saldoInsuficiente"
						bea:name="Saldo Insuficiente"></bpel:throw></bpel:sequence>
				<bpel:else>
					<bpel:sequence>
						<bpel:empty></bpel:empty></bpel:sequence></bpel:else></bpel:if></bpel:sequence>
        									</bpel:scope></bpel:sequence>
        									<bpel:else>
        										<bpel:sequence>
        											<bpel:empty></bpel:empty>
        										</bpel:sequence>
        									</bpel:else>
        								</bpel:if>
        								
        								
        								
        								
        								<bpel:if>
        									<bpel:condition>$validarEstado</bpel:condition>
        									<bpel:sequence>
        										<bpel:empty></bpel:empty>
        										<bpel:assign>
        											<bpel:extensionAssignOperation>
        												<bpel:documentation></bpel:documentation>
        												<trf:assign
        													varName="REQconsultadedetallesdelacuenta.parameters">
        													<trf:expr>
        														<expr:xqueryTransform>
        															<expr:resource
        																ref="Middleware/v2/BusinessServices/SJS/pagosMasivosInterno/xq/consultaDetalleDeCuentaIn"></expr:resource>
        															
        															<expr:param name="accountNumber">
        																<expr:path>data($batchAgrupado/ACCOUNT[xs:int($contadorCuentas)]/ACCOUNT_NUMBER)</expr:path></expr:param>
        															<expr:param name="sjPagosMasivosInterno">
        																<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        										<bpel:invoke
        											outputVariable="RSPconsultadedetalledelacuentaResponse"
        											partnerLink="consultaCuentasBS"
        											operation="Consultadedetallesdelacuenta"
        											inputVariable="REQconsultadedetallesdelacuenta">
        											<rescon:invokeInfo>
        												<rescon:service
        													ref="Middleware/Business_Resources/ConsultasCuenta/Resources/consultaCuentasBS"
        													isProxy="false"></rescon:service></rescon:invokeInfo></bpel:invoke>
        										<bpel:assign>
        											<bpel:extensionAssignOperation>
        												<trf:assign
        													varName="zeroRecords">
        													<trf:expr>
        														<expr:xqueryText>fn:string($RSPconsultadedetalledelacuentaResponse.parameters/Status/successIndicator/text()) = &quot;Success&quot; and
fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/ZERORECORDS/text()) = &quot;&quot;</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        										<bpel:if>
        											<bpel:condition>$zeroRecords</bpel:condition>
        											<bpel:sequence>
        												<bpel:empty></bpel:empty>
        												<bpel:assign>
        													<bpel:extensionAssignOperation>
        														<trf:assign
        															varName="inactiveMarker">
        															<trf:expr>
        																<expr:xqueryText>fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/gWSACCOUNTLISTDetailType/mWSACCOUNTLISTDetailType/INACTIVEMARKER/text())=&quot;Y&quot;</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
        													<bpel:extensionAssignOperation>
        														<trf:assign
        															varName="restriccion">
        															<trf:expr>
        																<expr:xqueryText>fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/gWSACCOUNTLISTDetailType/mWSACCOUNTLISTDetailType/POSTINGRESTRICT/text()) = &quot;DEBIT&quot;
or
fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/gWSACCOUNTLISTDetailType/mWSACCOUNTLISTDetailType/POSTINGRESTRICT/text()) = &quot;ALL&quot;</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        												<bpel:if>
        													<bpel:condition>$inactiveMarker 
or 
$restriccion</bpel:condition>
        													<bpel:sequence>
        														<bpel:empty></bpel:empty>
        														<bpel:assign>
        															<bpel:extensionAssignOperation>
        																<trf:replace
        																	varName="batchError" contents-only="true">
        																	<trf:expr>
        																		<expr:xqueryText>&lt;BATCH&gt;
	&lt;ID_POSITION&gt;{$contador}&lt;/ID_POSITION&gt;
	&lt;CUSTOMER_BATCH_ID&gt;{fn:string($request.inputParameters/BATCHES/BATCH[xs:int($contador)]/CUSTOMER_BATCH_ID/text())}&lt;/CUSTOMER_BATCH_ID&gt;
	&lt;STATUS&gt;ERROR&lt;/STATUS&gt;
	&lt;ERROR_CODE&gt;ERROR&lt;/ERROR_CODE&gt;
	&lt;ERROR_MESSAGE&gt;{fn:concat(&quot;La cuenta &quot;,data($batchAgrupado/ACCOUNT[xs:int($contadorCuentas)]/ACCOUNT_NUMBER), &quot; se encuentra inactiva o con restricciones&quot;)}&lt;/ERROR_MESSAGE&gt;
&lt;/BATCH&gt;</expr:xqueryText></trf:expr></trf:replace></bpel:extensionAssignOperation></bpel:assign>
