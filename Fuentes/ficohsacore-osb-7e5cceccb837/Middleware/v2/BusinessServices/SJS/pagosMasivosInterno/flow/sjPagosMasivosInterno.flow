        						<expr:xqueryText>&lt;NONE&gt;&lt;/NONE&gt;</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation></bpel:assign>
        			<bpel:invoke operation="validaEstructuraLote" partnerLink="validaEstructuraLote_db" outputVariable="RSPvalidaEstructuraLote" inputVariable="REQvalidaEstructuraLote">
