	       <bpel:variable name="response" messageType="ns2:sjPagosMasivosInternoResponse">
	       </bpel:variable>
    
    
    
    <bpel:variable name="totalBatches" type="xsd:integer"></bpel:variable>
    <bpel:variable name="Batches" type="xsd:anyType"></bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjPagosMasivosInterno" operation="PagosMasivosInterno" createInstance="yes" variable="request">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/v2/BusinessServices/SJS/pagosMasivosInterno/wsdl/sjPagosMasivosInterno" binding="ns2:sjPagosMasivosInternoPSSOAP"/>
            </rescon:receiveInfo>
        </bpel:receive>
        
        
        
        <bpel:assign>
