        			<bpel:variable name="zeroRecords" type="xsd:boolean">
        			</bpel:variable>
        			<bpel:variable name="inactiveMarker" type="xsd:boolean">
        			</bpel:variable>
        			<bpel:variable name="restriccion" type="xsd:boolean">
        			</bpel:variable>
        			<bpel:variable name="totalAmount"
        				type="xsd:decimal">
        			</bpel:variable>
        			<bpel:variable name="validaEstructura"
        				type="xsd:boolean">
        			</bpel:variable>
        			<bpel:variable name="batch" type="xsd:anyType"></bpel:variable>
        			<bpel:variable name="batchError" type="ns6:batchResponseType">
        			</bpel:variable>
        			<bpel:variable
        				name="REQconsultaDetallesCuentaMasivos"
        				messageType="ns7:consultaDetallesCuentaMasivosRequest">
        			</bpel:variable>
        			<bpel:variable
        				name="RSPconsultaDetallesCuentaMasivos"
        				messageType="ns7:consultaDetallesCuentaMasivosResponse">
        			</bpel:variable></bpel:variables>
        		
        		<bpel:faultHandlers>
        			<bpel:catch faultName="tns:saldoInsuficiente" ext:soapFaultVariable="soapFault">
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:assign>
		
	
	
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./BATCHES</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryText>$batchError/BATCH</expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        			</bpel:catch>
        			<bpel:catch faultName="tns:estadoInvalido" ext:soapFaultVariable="soapFault">
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:assign>
		
	
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./BATCHES</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryText>$batchError/BATCH</expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        			</bpel:catch>
        			<bpel:catchAll ext:soapFaultVariable="soapFault">
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				
	<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation></bpel:documentation>
			<trf:replace contents-only="true" varName="batchError">
				<trf:expr>
					<expr:xqueryText>&lt;BATCH&gt;
	&lt;ID_POSITION&gt;{$contador}&lt;/ID_POSITION&gt;
	&lt;CUSTOMER_BATCH_ID&gt;{fn:string($request.inputParameters/BATCHES/BATCH[xs:int($contador)]/CUSTOMER_BATCH_ID/text())}&lt;/CUSTOMER_BATCH_ID&gt;
	&lt;STATUS&gt;ERROR&lt;/STATUS&gt;
	&lt;ERROR_CODE&gt;ERROR&lt;/ERROR_CODE&gt;
	&lt;ERROR_MESSAGE&gt;Error inesperado&lt;/ERROR_MESSAGE&gt;
&lt;/BATCH&gt;</expr:xqueryText></trf:expr></trf:replace></bpel:extensionAssignOperation>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./BATCHES</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryText>$batchError/BATCH</expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        			</bpel:catchAll></bpel:faultHandlers>
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="REQvalidaEstructuraLote.InputParameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/v2/BusinessServices/SJS/pagosMasivosInterno/xq/validaEstructuraLoteIn"></expr:resource>
        								
        								
        								<expr:param name="numerobatch">
        									<expr:path>$contador</expr:path></expr:param>
        								<expr:param name="sjPagosMasivosInterno1">
        									<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			<bpel:extensionAssignOperation>
		<trf:assign varName="batchError">
			<trf:expr>
				<expr:xqueryText>&lt;NONE&gt;&lt;/NONE&gt;</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="batch">
