<?xml version="1.0" encoding="UTF-8"?>
<bpel:process name="sjConsultaTipoTarjeta"
              targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/flow/sjConsultaTipoTarjeta"
              xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/flow/sjConsultaTipoTarjeta"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:rescon="http://www.bea.com/alsb/flow/resources/config"
              xmlns:bea="http://www.bea.com/bpel/ui/extensions"
              xmlns:ext="http://www.bea.com/bpel/extensions"
              xmlns:expr="http://www.bea.com/wli/sb/stages/config"
              xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config"
              xmlns:bind="http://www.ficohsa.com.hn/middleware.services/sjConsultaTipoTarjetaPS" 
              bea:name="sjConsultaTipoTarjeta" xmlns:ns1="T24WebServicesImpl" xmlns:ns0="http://xmlns.oracle.com/pcbpel/adapter/db/ConsultarBines/ConsultarBines/consultarBines">
    <bpel:partnerLinks>
        <bpel:partnerLink name="sjConsultaTipoTarjeta" partnerLinkType="tns:sjConsultaTipoTarjeta"
                          myRole="sjConsultaTipoTarjeta">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="tarjetasDebitoBS"
		partnerLinkType="tns:tarjetasDebitoBS_plnkType"
		partnerRole="tarjetasDebitoBS_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="consultarBinesTC_db"
	   	partnerLinkType="tns:consultarBinesTC_db_plnkType"
	   	partnerRole="consultarBinesTC_db_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request"
	                      messageType="bind:sjConsultaTipoTarjetaRequest">
	       </bpel:variable>
	       <bpel:variable name="response"
                       messageType="bind:sjConsultaTipoTarjetaResponse">
	       </bpel:variable>
    <bpel:variable name="REQConsBinesTD"
		messageType="ns1:ConsultadebinesTD">
    </bpel:variable>
    <bpel:variable name="RSPConsBinesTD"
    	messageType="ns1:ConsultadebinesTDResponse">
    </bpel:variable>
    <bpel:variable name="REQConsBinesTC"
    	messageType="ns0:args_in_msg">
    </bpel:variable>
    <bpel:variable name="RSPConsBinesTC"
    	messageType="ns0:args_out_msg">
    </bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaTipoTarjeta" operation="ConsultaTipoTarjeta" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/wsdl/sjConsultaTipoTarjeta" binding="bind:sjConsultaTipoTarjetaPSSOAP"/>
            </rescon:receiveInfo>
        </bpel:receive>
        <bpel:flow>
        	<bpel:scope bea:name="BinesTD">
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="REQConsBinesTD.parameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/xq/sjConsultaBinesTDIn"></expr:resource>
        								<expr:param name="sjConsultaTipoTarjeta1">
        									<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign>
        			<bpel:invoke partnerLink="tarjetasDebitoBS"
        				outputVariable="RSPConsBinesTD"
        				operation="ConsultadebinesTD"
        				inputVariable="REQConsBinesTD">
        				<rescon:invokeInfo>
        					<rescon:service isProxy="false"
        						ref="Middleware/Business_Resources/tarjetasDebito/Resources/tarjetasDebitoBS"></rescon:service></rescon:invokeInfo></bpel:invoke></bpel:sequence>
        	</bpel:scope>
        	<bpel:scope bea:name="BinesTC">
        		
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="REQConsBinesTC.InputParameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/xq/sjConsultaBinesTCIn"></expr:resource>
        								<expr:param name="sjConsultaTipoTarjeta1">
        									<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign>
        			<bpel:invoke operation="consultarBines" partnerLink="consultarBinesTC_db" inputVariable="REQConsBinesTC" outputVariable="RSPConsBinesTC">
        				<rescon:invokeInfo>
        					<rescon:service isProxy="false" ref="Middleware/v2/BusinessServices/ABK/consultarBinesTC/biz/consultarBinesTC_db"></rescon:service></rescon:invokeInfo></bpel:invoke></bpel:sequence>
        	</bpel:scope>
        </bpel:flow>
        
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.outputParameters">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTipoTarjeta/xq/sjConsultarTipoTarjetaOut"></expr:resource>
        					<expr:param name="consultadebinesTDResponse1">
        						<expr:path>$RSPConsBinesTD.parameters</expr:path></expr:param>
        					<expr:param name="outputParameters1">
        						<expr:path>$RSPConsBinesTC.OutputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        	
        </bpel:assign>
        <bpel:reply partnerLink="sjConsultaTipoTarjeta" operation="ConsultaTipoTarjeta" variable="response"></bpel:reply>
    </bpel:sequence>
</bpel:process>