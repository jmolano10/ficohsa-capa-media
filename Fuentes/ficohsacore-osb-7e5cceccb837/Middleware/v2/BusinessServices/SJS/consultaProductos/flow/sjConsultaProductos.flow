<?xml version="1.0" encoding="UTF-8"?>
<bpel:process name="sjConsultaProductos"
              targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaProductos/flow/sjConsultaProductos"
              xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaProductos/flow/sjConsultaProductos"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:rescon="http://www.bea.com/alsb/flow/resources/config"
              xmlns:bea="http://www.bea.com/bpel/ui/extensions"
              xmlns:ext="http://www.bea.com/bpel/extensions"
              xmlns:expr="http://www.bea.com/wli/sb/stages/config"
              xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config"
              xmlns:bind="http://www.ficohsa.com.hn/middleware.services/sjConsultaProductos" 
              bea:name="sjConsultaProductos" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="T24WebServicesImpl" xmlns:ns0="http://xmlns.oracle.com/pcbpel/adapter/db/InterbancaSOAApplication/ConsultaGeneralPrestamo/consultaGeneralPrestamo">
    <bpel:partnerLinks>
        <bpel:partnerLink name="sjConsultaProductos" partnerLinkType="tns:sjConsultaProductos"
                          myRole="sjConsultaProductos">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="consultaCuentasBS"
		partnerLinkType="tns:consultaCuentasBS_plnkType"
		partnerRole="consultaCuentasBS_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="consultaGeneralPrestamoBS"
	   	partnerLinkType="tns:consultaGeneralPrestamoBS_plnkType"
	   	partnerRole="consultaGeneralPrestamoBS_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request"
	                      messageType="bind:sjConsultaProductosRequest">
	       </bpel:variable>
	       <bpel:variable name="response"
                       messageType="bind:sjConsultaProductosResponse">
	       </bpel:variable>
    <bpel:variable name="accountCount" type="xsd:integer"></bpel:variable>
    <bpel:variable name="loanCount" type="xsd:integer"></bpel:variable>
    <bpel:variable name="accountList" type="xsd:string">
    </bpel:variable>
    <bpel:variable name="loanList" type="xsd:string"></bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaProductos" operation="consultaProductos" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/v2/BusinessServices/SJS/consultaProductos/wsdl/sjConsultaProductos" binding="bind:sjConsultaProductosPSSOAP"/>
            </rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.outputParameters">
        			<trf:expr>
        				<expr:xqueryText>&lt;bind:sjConsultaProductosResponse&gt;
	&lt;bind:PRODUCTS/&gt;
&lt;/bind:sjConsultaProductosResponse&gt;</expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        
	
	
	<bpel:extensionAssignOperation>
		<bpel:documentation></bpel:documentation>
		<trf:assign varName="accountCount">
			<trf:expr>
				<expr:xqueryText>fn:count($request.inputParameters/bind:PRODUCTS/bind:PRODUCT[bind:TYPE=&quot;CTA&quot;])</expr:xqueryText></trf:expr></trf:assign>
	</bpel:extensionAssignOperation>
	<bpel:extensionAssignOperation>
		<bpel:documentation></bpel:documentation>
		<trf:assign varName="loanCount">
			<trf:expr>
				<expr:xqueryText>fn:count($request.inputParameters/bind:PRODUCTS/bind:PRODUCT[bind:TYPE=&quot;PTM&quot;])</expr:xqueryText></trf:expr></trf:assign>
	</bpel:extensionAssignOperation></bpel:assign>
        
        
        
        <bpel:flow>
        	<bpel:scope bea:name="Cuentas">
        		
        		<bpel:sequence>
        			<bpel:if>
        				<bpel:condition>$accountCount &gt; 0</bpel:condition>
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation></bpel:documentation>
			<trf:assign varName="accountList">
				<trf:expr>
					<expr:xqueryText>fn:string-join($request.inputParameters/bind:PRODUCTS/bind:PRODUCT[bind:TYPE=&quot;CTA&quot;]/bind:ID/text(),&quot;,&quot;)</expr:xqueryText></trf:expr></trf:assign>
		</bpel:extensionAssignOperation>
        				</bpel:assign>
	<bpel:forEach parallel="yes" counterName="accountCounter">
		<bpel:startCounterValue>number(1)</bpel:startCounterValue>
		<bpel:finalCounterValue>number($accountCount)</bpel:finalCounterValue>
		<bpel:scope>
			<bpel:variables>
				<bpel:variable name="accountNumber" type="xsd:string"></bpel:variable>
				<bpel:variable name="REQConsCuenta"
					messageType="ns1:Consultadedetallesdelacuenta">
				</bpel:variable>
				<bpel:variable name="RSPConsCuenta"
					messageType="ns1:ConsultadedetallesdelacuentaResponse">
				</bpel:variable></bpel:variables>
			<bpel:faultHandlers>
				
				<bpel:catchAll>
					<bpel:sequence>
						<bpel:empty></bpel:empty>
					<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./bind:PRODUCTS</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:PRODUCT&gt;
	&lt;bind:TYPE&gt;CTA&lt;/bind:TYPE&gt;
	&lt;bind:TYPE&gt;{ $accountNumber }&lt;/bind:TYPE&gt;
	&lt;bind:SUCCESS_INDICATOR&gt;ERROR&lt;/bind:SUCCESS_INDICATOR&gt;
	&lt;bind:MESSAGE&gt;Error obteniendo informacion de cuenta&lt;/bind:MESSAGE&gt;
&lt;/bind:PRODUCT&gt;</expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
				</bpel:catchAll></bpel:faultHandlers>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:assign varName="accountNumber">
							<trf:expr>
								<expr:xqueryText>fn:tokenize($accountList,&quot;,&quot;)[xs:int($accountCounter)]</expr:xqueryText></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
					<bpel:extensionAssignOperation>
						<trf:assign varName="REQConsCuenta.parameters">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/Resources/Genericos/consultaDetalleCuentaHNIn"></expr:resource>
									
									
									
									
									
									
									
									
									<expr:param name="password">
										<expr:path>fn:string($request.inputParameters/bind:LDAP_PASSWORD/text())</expr:path></expr:param>
									<expr:param name="username">
										<expr:path>fn:string($request.inputParameters/bind:LDAP_USERNAME/text())</expr:path></expr:param>
									<expr:param name="accountNumber">
										<expr:path>$accountNumber</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
				</bpel:assign>
				<bpel:invoke operation="Consultadedetallesdelacuenta"
					outputVariable="RSPConsCuenta" partnerLink="consultaCuentasBS"
					inputVariable="REQConsCuenta">
					<rescon:invokeInfo>
						<rescon:service
							ref="Middleware/Business_Resources/ConsultasCuenta/Resources/consultaCuentasBS"
							isProxy="false"></rescon:service></rescon:invokeInfo></bpel:invoke>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:insert
							varName="response.outputParameters">
							<trf:location>
								<expr:xpathText>./bind:PRODUCTS</expr:xpathText></trf:location>
							<trf:where>last-child</trf:where>
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource
										ref="Middleware/v2/BusinessServices/SJS/consultaProductos/xq/sjConsultaCuentasOut"></expr:resource>
									
									
									
									
									<expr:param name="accountNumber">
										<expr:path>$accountNumber</expr:path></expr:param>
									<expr:param name="consultadedetallesdelacuentaResponse1">
										<expr:path>$RSPConsCuenta.parameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
		</bpel:scope>
        				</bpel:forEach>
	
        				
        				
        				</bpel:sequence>
        				<bpel:else>
        					<bpel:sequence>
        						<bpel:empty></bpel:empty>
        					</bpel:sequence>
        				</bpel:else>
        			</bpel:if></bpel:sequence>
        	</bpel:scope>
        	<bpel:scope bea:name="Prestamos">
        		
        		<bpel:sequence>
        			<bpel:if>
        				<bpel:condition>$loanCount &gt; 0</bpel:condition>
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation></bpel:documentation>
			<trf:assign varName="loanList">
				<trf:expr>
					<expr:xqueryText>fn:string-join($request.inputParameters/bind:PRODUCTS/bind:PRODUCT[bind:TYPE=&quot;PTM&quot;]/bind:ID/text(),&quot;,&quot;)</expr:xqueryText></trf:expr></trf:assign>
		</bpel:extensionAssignOperation>
        				</bpel:assign>
	<bpel:forEach parallel="yes" counterName="loanCounter">
		<bpel:startCounterValue>number(1)</bpel:startCounterValue>
		<bpel:finalCounterValue>number($loanCount)</bpel:finalCounterValue>
		<bpel:scope>
			<bpel:variables>
				<bpel:variable name="loanNumber" type="xsd:string"></bpel:variable>
				<bpel:variable name="RSPConsPtmo"
					messageType="ns0:args_out_msg">
				</bpel:variable>
				<bpel:variable name="REQConsPtmo"
					messageType="ns0:args_in_msg">
				</bpel:variable></bpel:variables>
			<bpel:faultHandlers>
				<bpel:catchAll>
					<bpel:sequence>
						<bpel:empty></bpel:empty>
					<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./bind:PRODUCTS</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:PRODUCT&gt;
	&lt;bind:TYPE&gt;PTM&lt;/bind:TYPE&gt;
	&lt;bind:TYPE&gt;{ $loanNumber }&lt;/bind:TYPE&gt;
	&lt;bind:SUCCESS_INDICATOR&gt;ERROR&lt;/bind:SUCCESS_INDICATOR&gt;
	&lt;bind:MESSAGE&gt;Error obteniendo informacion de prestamo&lt;/bind:MESSAGE&gt;
&lt;/bind:PRODUCT&gt;</expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
				</bpel:catchAll></bpel:faultHandlers>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:assign varName="loanNumber">
							<trf:expr>
								<expr:xqueryText>fn:tokenize($loanList,&quot;,&quot;)[xs:int($loanCounter)]</expr:xqueryText></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
					<bpel:extensionAssignOperation>
						<trf:assign
							varName="REQConsPtmo.InputParameters">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource
										ref="Middleware/v2/Resources/Genericos/consultaPtmoAbanksHNIn"></expr:resource>
									
									<expr:param name="loanNumber">
	<expr:path>$loanNumber</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
				<bpel:invoke operation="consultaGeneralPrestamo"
					outputVariable="RSPConsPtmo"
					partnerLink="consultaGeneralPrestamoBS"
					inputVariable="REQConsPtmo">
					<rescon:invokeInfo>
						<rescon:service
							ref="Middleware/Business_Resources/consultaGeneralPrestamo/Resources/consultaGeneralPrestamoBS"
							isProxy="false"></rescon:service></rescon:invokeInfo></bpel:invoke>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:insert
							varName="response.outputParameters">
							<trf:location>
								<expr:xpathText>./bind:PRODUCTS</expr:xpathText></trf:location>
							<trf:where>last-child</trf:where>
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource
										ref="Middleware/v2/BusinessServices/SJS/consultaProductos/xq/sjConsultaPtmoOut"></expr:resource>
									
									
									<expr:param name="loanNumber">
										<expr:path>$loanNumber</expr:path></expr:param>
									<expr:param name="outputParameters1">
										<expr:path>$RSPConsPtmo.OutputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
		</bpel:scope>
        				</bpel:forEach>
	
	
	</bpel:sequence>
        				<bpel:else>
        					<bpel:sequence>
        						<bpel:empty></bpel:empty>
        					</bpel:sequence>
        				</bpel:else>
        			</bpel:if></bpel:sequence>
        	</bpel:scope>
        </bpel:flow>
        <bpel:reply partnerLink="sjConsultaProductos" operation="consultaProductos" variable="response"></bpel:reply>
    </bpel:sequence>
</bpel:process>