         </sequence>
      </complexType>
   </element>
   <complexType name="TERCEROSPA.TDATA">
      <sequence>
         <element name="PT_INFODATA_ITEM" type="db:TERCEROSPA.DATA" db:type="Struct" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
      </sequence>
   </complexType>
   <complexType name="TERCEROSPA.DATA">
      <sequence>
         <element name="NAME" type="db:string100" db:type="VARCHAR2" minOccurs="0" nillable="true"/>
         <element name="VALUE" type="db:string5000" db:type="VARCHAR2" minOccurs="0" nillable="true"/>
      </sequence>
   </complexType>
   <simpleType name="string100">
      <restriction base="string">
         <maxLength value="100"/>
      </restriction>
   </simpleType>
   <simpleType name="string5000">
      <restriction base="string">
         <maxLength value="5000"/>
      </restriction>
   </simpleType>
