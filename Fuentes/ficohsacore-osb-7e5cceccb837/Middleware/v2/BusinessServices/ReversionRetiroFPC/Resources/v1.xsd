<xsd:schema targetNamespace="https://www.ficohsa.com/regional/pension" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="https://www.ficohsa.com/regional/pension" xmlns:ctp="https://www.ficohsa.com/regional/common/commonTypes">
