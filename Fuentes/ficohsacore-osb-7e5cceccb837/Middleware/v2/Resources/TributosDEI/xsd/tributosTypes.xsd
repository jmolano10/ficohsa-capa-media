						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="RTN" type="tri:stringMaxLength20" />
				<xs:element name="EMAIL" type="tri:stringMinLength1" />
				<xs:element name="BUILDING_DATE" type="xs:date"
					minOccurs="0" />
				<xs:element name="BUSINESS_NAME" type="tri:stringMaxLength80"
					minOccurs="0" />
				<xs:element name="LEGAL_REP_ID" type="tri:stringMax20Length"
					minOccurs="0" />
				<xs:element name="LEGAL_REP_NAME" minOccurs="0"
					type="tri:stringMaxLength80" />
				<xs:element name="DEED_NUMBER" type="tri:stringMax20Length"
					minOccurs="0" />
				<xs:element name="DEED_DATE" type="xs:date" minOccurs="0" />
				<xs:element name="DJ_LAST_NUMBER" type="tri:stringMax20Length"
					minOccurs="0" />
