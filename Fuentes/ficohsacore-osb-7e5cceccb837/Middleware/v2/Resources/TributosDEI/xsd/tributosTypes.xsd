<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:tri="http://www.ficohsa.com.hn/middleware.services/tributosTypes"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/tributosTypes">


	<!-- OPERACION: CONSULTA CARGA ARCHIVO DAT -->

	<xs:complexType name="ConsultaCargaArchivoDATResponse">
		<xs:sequence>
			<xs:element name="BANK_BATCH_ID" type="xs:string"
				minOccurs="0" />
			<xs:element name="RTN" type="xs:string" minOccurs="0" />
			<xs:element name="PRESENTATION_DATE" type="xs:date"
				minOccurs="0" />
			<xs:element name="DECLARATION_ID" type="xs:string"
				minOccurs="0" />
			<xs:element name="PERIOD" type="xs:string" minOccurs="0" />
			<xs:element name="TAX_ID" type="xs:integer" minOccurs="0" />
			<xs:element name="TAX_DESCRIPTION" type="xs:string" minOccurs="0" />
			<xs:element name="CONCEPT_ID" type="xs:integer" minOccurs="0" />
	        <xs:element name="CONCEPT_DESCRIPTION" type="xs:string" minOccurs="0" />
			<xs:element name="TAX_PAYMENT" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="PENALTY_FEE" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="SURCHARGE_PAYMENT" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="TAX_INTEREST" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="TOTAL_PAYMENT" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="TOTAL_PRESENTED" type="xs:decimal"
				minOccurs="0" />
			<xs:element name="PAYMENT_DETAILS" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PAYMENT_DETAIL" type="tri:PaymentDetailType"
							minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>

		</xs:sequence>

	</xs:complexType>

	<!--############################### -->
	<!--TYPES: TRIBUTOS -->
	<!-- StringType -->
	<xs:simpleType name="stringMinLength1">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
		</xs:restriction>
	</xs:simpleType>

	<!-- StringType -->
	<xs:simpleType name="stringMaxLength20">
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<!-- StringType max30 -->
	<xs:simpleType name="stringMaxLength30">
		<xs:restriction base="xs:string">
			<xs:maxLength value="30" />
		</xs:restriction>
	</xs:simpleType>

	<!-- StringType max20 -->
	<xs:simpleType name="stringMax20Length">
		<xs:restriction base="xs:string">
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<!-- StringType max80 -->
	<xs:simpleType name="stringMaxLength80">
		<xs:restriction base="xs:string">
			<xs:maxLength value="80" />
		</xs:restriction>
	</xs:simpleType>

	<!--############################### -->
	<!-- YESNO TYPE -->
	<xs:simpleType name="yesNoType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="YES" />
			<xs:enumeration value="NO" />
		</xs:restriction>
	</xs:simpleType>

	<!--############################### -->
	<!-- PERSON TYPE -->
	<xs:simpleType name="personType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="N" />
			<xs:enumeration value="J" />
		</xs:restriction>
	</xs:simpleType>

	<!--############################### -->
	<!-- REQUEST TYPE -->
	<xs:simpleType name="requestType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PRESENT" />
			<xs:enumeration value="PAYMENT" />
		</xs:restriction>
	</xs:simpleType>



	<!--############################### -->
	<!--OPERACION: CONSULTA NUMERO DE CONTRATO -->
	<!--############################### -->
	<xs:element name="consultarNroContrato">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RTN" type="tri:stringMaxLength20" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="consultarNroContratoResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RTN" type="xs:string" minOccurs="0" />
				<xs:element name="CONTRACT_NUMBER" type="xs:string"
					minOccurs="0" />
				<xs:element name="CONTRACT_STATUS" type="xs:integer"
					minOccurs="0" />
				<xs:element name="CONTRACT_STATUS_DESC" type="xs:string"
					minOccurs="0" />
				<xs:element name="CLIENT_TYPE" type="xs:string"
					minOccurs="0" />
				<xs:element name="TITLE_NAME" type="xs:string" minOccurs="0" />
				<xs:element name="TITLE_ID" type="xs:string" minOccurs="0" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<!--############################### -->
	<!--OPERACION: ALTA DE CONTRATO -->
	<!--############################### -->
	<xs:element name="altaContrato">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RTN" type="tri:stringMaxLength20" />
				<xs:element name="CONTRACT_NUMBER" type="tri:stringMinLength1" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="altaContratoResponse" />


	<!--############################### -->
	<!--OPERACION: OBTENER DATOS DE CONTRATO -->
	<!--############################### -->
	<xs:element name="obtenerDatosContrato">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RTN" type="tri:stringMaxLength20" />
				<xs:element name="CONTRACT_NUMBER" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="obtenerDatosContratoResponse">
		<xs:complexType>
