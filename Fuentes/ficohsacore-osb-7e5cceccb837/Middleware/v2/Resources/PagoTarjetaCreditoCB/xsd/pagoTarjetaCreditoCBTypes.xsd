				<xs:element name="PAYER_NAME" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="pagoTarjetaCreditoResponse" type="cred:pagoTarjetaCreditoResponseType"/>
	
	
</xs:schema>