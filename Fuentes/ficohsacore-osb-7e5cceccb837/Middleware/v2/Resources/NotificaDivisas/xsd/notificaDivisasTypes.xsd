					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
    </xs:element>
    
    <xs:complexType name = "headerList">
			<xs:sequence>
				<xs:element name="INSTITUTION_CODE" type="xs:string" />
				<xs:element name="DATE" type="xs:string" />
