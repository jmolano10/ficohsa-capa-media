				<xs:element name="OWN_INSTITUTION_AMOUNT_EXPENSES" type="xs:string"/>
				<xs:element name="TOTAL_AMOUNT_EXPENSES" type="xs:string" />
				<xs:element name="OFFICIAL_NAME" type="xs:string" />
			</xs:sequence>
	</xs:complexType>
	
