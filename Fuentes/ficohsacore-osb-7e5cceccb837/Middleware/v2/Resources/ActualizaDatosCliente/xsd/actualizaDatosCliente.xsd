	xmlns:actd="http://www.ficohsa.com.hn/middleware.services/actualizaDatosClienteTypes/v2">


	<xs:complexType name="actualizaDatosClienteRequest">
		<xs:sequence>
			<xs:element name="CUSTOMER_ID" type="xs:string"/>
			<xs:element name="PRIMARY_EMAIL" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="actualizaDatosClienteResponse">
