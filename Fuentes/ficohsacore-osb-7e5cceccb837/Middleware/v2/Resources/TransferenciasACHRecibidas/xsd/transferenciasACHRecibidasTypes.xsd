<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	version="1.0"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/transferenciasACHRecibidasTypes"
	xmlns:trans="http://www.ficohsa.com.hn/middleware.services/transferenciasACHRecibidasTypes">

	<xs:element name="transferenciasACHRecibidas">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="GLOBAL_REFERENCE" type="xs:string"/>
				<xs:element name="TRANSACTION_DETAILS" type="trans:transactionDetails"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="transactionDetails">
		<xs:sequence>
			<xs:element name="TRANSACTION_DETAIL" type="trans:transactionDetail" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="transactionDetail">
		<xs:sequence>
			<xs:element name="TRANSACTION_TYPE"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:enumeration value="ACCT_DEBIT"/>
	        			<xs:enumeration value="ACCT_CREDIT"/>
	        			<xs:enumeration value="CREDIT_CARD_PAYMENT"/>
	        			<xs:enumeration value="LOAN_PAYMENT"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>
			<xs:element name="DESTINATION"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:minLength value="1"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>			
			<xs:element name="CURRENCY"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:minLength value="1"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>
			<xs:element name="AMOUNT"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:minLength value="1"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>
			<xs:element name="BENEFICIARY_NAME"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:minLength value="1"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>
			<xs:element name="ORIGINATOR_INFO" type="trans:originatorInfo" minOccurs="1" maxOccurs="1"/>
			<xs:element name="TRANSACTION_RESULT" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SUCCESS_INDICATOR" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="TRANSACTION_ID" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="ERROR_CODE" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="ERROR_MESSAGE" type="xs:string" minOccurs="0" maxOccurs="1"/>									
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BENEFICIARY_ID" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="originatorInfo">
		<xs:sequence>
			<xs:element name="BANK_ID"> 
				<xs:simpleType>
	        		<xs:restriction base="xs:string">
	        			<xs:minLength value="1"/>
	        		</xs:restriction>
        		</xs:simpleType>
			</xs:element>
			<xs:element name="ACCOUNT" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="REFERENCE_NUMBER" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="CUSTOMER_NAME" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DESCRIPTION" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name="transferenciasACHRecibidasResponse" type="trans:transferenciasACHRecibidasResponseType" />

	<xs:complexType name="transferenciasACHRecibidasResponseType">
		<xs:sequence>
			<xs:element name="VALUE_DATE" type="xs:string" minOccurs="0"/>
			<xs:element name="TRANSACTION_DETAILS" type="trans:transactionDetails" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

</xs:schema>