<wsdl:definitions 
name="consultaNroContratoDEIPS"
	targetNamespace="http://www.ficohsa.com.hn/middleware.services/consultaDGIPS/"
	xmlns:fic="http://www.ficohsa.com.hn/middleware.services/consultaDGIPS/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	xmlns:auth="http://www.ficohsa.com.hn/middleware.services/autType"
	xmlns:cons="http://www.ficohsa.com.hn/middleware.services/consultaDGITypes">

	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://www.ficohsa.com.hn/middleware.services/autType"
				schemaLocation="../../esquemas_generales/HeaderElements.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import
				namespace="http://www.ficohsa.com.hn/middleware.services/consultaDGITypes"
				schemaLocation="../../ConsultaDGI/xsd/consultaDGITypes.xsd" />
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="consultaDGIRequest">
		<wsdl:part element="cons:consultaDGI" name="parameters" />
		<wsdl:part name="RequestHeader" element="auth:RequestHeader" />
	</wsdl:message>
	<wsdl:message name="consultaDGIResponse">
		<wsdl:part element="cons:consultaDGIResponse" name="result" />
		<wsdl:part name="ResponseHeader" element="auth:ResponseHeader" />
	</wsdl:message>

	<wsdl:portType name="consultaDGIPS">
		<wsdl:operation name="consultaDGI"
			parameterOrder="parameters RequestHeader ResponseHeader">
			<wsdl:input message="fic:consultaDGIRequest" />
			<wsdl:output message="fic:consultaDGIResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="consultaDGIPSSOAP" type="fic:consultaDGIPS">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="consultaDGI">
			<soap:operation
				soapAction="http://www.ficohsa.com.hn/middleware.services/consultaDGIPS/consultaDGI" />
			<wsdl:input>
				<soap:body use="literal" parts="parameters" />
				<soap:header message="fic:consultaDGIRequest"
					part="RequestHeader" use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" parts="result" />
				<soap:header message="fic:consultaDGIResponse"
					part="ResponseHeader" use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="consultaDGIPS">
		<wsdl:port binding="fic:consultaDGIPSSOAP" name="consultaDGIPSSOAP">
			<soap:address
				location="http://172.23.13.19:8003/Middleware/ReferenceData/BusinessPartner/consultaDGI" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>