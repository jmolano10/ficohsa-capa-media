<?xml version="1.0" encoding="UTF-8"?>
<con:schemaEntry xmlns:con="http://www.bea.com/wli/sb/resources/config">
    <con:schema><![CDATA[<xs:schema version="1.0" targetNamespace="http://www.ficohsa.com.hn/middleware.services/transferenciasType" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:trans="http://www.ficohsa.com.hn/middleware.services/transferenciasType" xmlns:kvp="http://www.ficohsa.com.hn/middleware.services/kvpType">
	
	<xs:import namespace="http://www.ficohsa.com.hn/middleware.services/kvpType" schemaLocation="../../esquemas_generales/KeyValuePair.xsd"/>
	<xs:complexType name="transferenciaCuentasRequest">
		<xs:sequence>
			<xs:element name="DEBIT_ACCOUNT" type="xs:string"/>
			<xs:element name="CREDIT_ACCOUNT" type="xs:string"/>
			<xs:element name="CURRENCY" type="xs:string" minOccurs="0"/>
			<xs:element name="TRANSFER_AMOUNT" type="xs:string"/>
			<xs:element name="DEBIT_CREDIT" type="xs:string" minOccurs="0"/>
			<xs:element name="DESCRIPTION" type="xs:string" minOccurs="0"/>
			<xs:element name="AUTHORIZATION_TICKET" type="xs:string" minOccurs="0"/>
			<xs:element name="TRANSFER_DETAILS" type="xs:string" minOccurs="0"/>
			<xs:element name="INTERFACE_REFERENCE_NO" type="xs:string" minOccurs="0"/>
			<xs:element name="ITEM_LEVEL_1" type="xs:string" minOccurs="0"/>
			<xs:element name="ITEM_LEVEL_2" type="xs:string" minOccurs="0"/>
			<xs:element name="ITEM_LEVEL_3" type="xs:string" minOccurs="0"/>
			<xs:element name="ITEM_LEVEL_4" type="xs:string" minOccurs="0"/>
			<xs:element name="ITEM_LEVEL_5" type="xs:string" minOccurs="0"/>
			<xs:element name="REGIONAL_DETAILS" type="kvp:RegionalDetails" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ADDITIONAL_INFO" type="kvp:UnboundKvPair" minOccurs="0" maxOccurs="1"/>			
		</xs:sequence>
	</xs:complexType>
	

	<xs:complexType name="transferenciaCuentasResponse">
		<xs:sequence>
			<xs:element name="DEBIT_ACCOUNT" type="xs:string" minOccurs="0"/>
			<xs:element name="CREDIT_ACCOUNT" type="xs:string" minOccurs="0"/>
			<xs:element name="DEBIT_CUSTOMER" type="xs:string" minOccurs="0"/>
			<xs:element name="CREDIT_CUSTOMER" type="xs:string" minOccurs="0"/>
			<xs:element name="DEBIT_CURRENCY" type="xs:string" minOccurs="0"/>
			<xs:element name="CREDIT_CURRENCY" type="xs:string" minOccurs="0"/>
			<xs:element name="DEBIT_AMOUNT" type="xs:string" minOccurs="0"/>
			<xs:element name="CREDIT_AMOUNT" type="xs:string" minOccurs="0"/>
			<xs:element name="TRANSFER_DETAILS" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EXCHANGE_RATE" type="xs:string" minOccurs="0"/>
			<xs:element name="DATE_TIME" type="xs:string" minOccurs="0"/>
		    <xs:element name="INTERNATIONAL_ACCOUNT_NUMBER_DEBIT_ACCOUNT" type="xs:string" minOccurs="0" />
            <xs:element name="INTERNATIONAL_ACCOUNT_NUMBER_CREDIT_ACCOUNT" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="transferenciaCuentasRequest" type="trans:transferenciaCuentasRequest"/>

	<xs:element name="transferenciaCuentasResponse" type="trans:transferenciaCuentasResponse"/>

</xs:schema>]]></con:schema>
    <con:dependencies>
        <con:import namespace="http://www.ficohsa.com.hn/middleware.services/kvpType" schemaLocation="../../esquemas_generales/KeyValuePair.xsd" ref="Middleware/v2/Resources/esquemas_generales/KeyValuePair"/>
    </con:dependencies>
    <con:targetNamespace>http://www.ficohsa.com.hn/middleware.services/transferenciasType</con:targetNamespace>
</con:schemaEntry>