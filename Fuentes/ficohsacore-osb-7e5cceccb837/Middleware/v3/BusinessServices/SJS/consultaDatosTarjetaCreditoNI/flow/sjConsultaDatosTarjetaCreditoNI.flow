<?xml version="1.0" encoding="UTF-8"?>
<bpel:process name="sjConsultaDatosTarjetaCreditoNI"
              targetNamespace="http://www.example.com/flow/Middleware/v3/BusinessServices/SJS/consultaDatosTarjetaCreditoNI/flow/sjConsultaDatosTarjetaCreditoNI"
              xmlns:tns="http://www.example.com/flow/Middleware/v3/BusinessServices/SJS/consultaDatosTarjetaCreditoNI/flow/sjConsultaDatosTarjetaCreditoNI"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:rescon="http://www.bea.com/alsb/flow/resources/config"
              xmlns:bea="http://www.bea.com/bpel/ui/extensions"
              xmlns:ext="http://www.bea.com/bpel/extensions"
              xmlns:expr="http://www.bea.com/wli/sb/stages/config"
              xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config"
              xmlns:bind="http://www.ficohsa.com.hn/middleware.services/sjconsultaDatosTarjetaCreditoPS/" 
              bea:name="sjConsultaDatosTarjetaCreditoNI" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://tempuri.org/Procesa" xmlns:con="http://www.ficohsa.com.hn/middleware.services/sjconsultaDatosTarjetaCreditoTypes">
    <bpel:partnerLinks>
        <bpel:partnerLink name="sjConsultaDatosTarjetaCreditoNI" partnerLinkType="tns:sjConsultaDatosTarjetaCreditoNI"
                          myRole="sjConsultaDatosTarjetaCreditoNI">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="vmx" partnerLinkType="tns:vmx_plnkType"
		partnerRole="vmx_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request"
	                      messageType="bind:sjconsultaDatosTarjetaCredito">
	       </bpel:variable>
	       <bpel:variable name="response"
                       messageType="bind:sjconsultaDatosTarjetaCreditoResponse">
	       </bpel:variable>
    <bpel:variable name="cardNumber" type="xsd:string"></bpel:variable>
    <bpel:variable name="resultGroup" type="xsd:anyType"></bpel:variable>
    <bpel:variable name="org" type="xsd:string"></bpel:variable>
    <bpel:variable name="orgType" type="xsd:string"></bpel:variable>
    <bpel:variable name="orgCount" type="xsd:int"></bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaDatosTarjetaCreditoNI" operation="sjconsultaDatosTarjetaCredito" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/v3/BusinessServices/SJS/consultaDatosTarjetaCredito/wsdl/sjconsultaDatosTarjetaCreditoPS" binding="bind:sjconsultaDatosTarjetaCreditoPSSOAP"/>
            </rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	
        <bpel:extensionAssignOperation>
		<trf:assign varName="resultGroup">
			<trf:expr>
				<expr:xqueryText>&lt;RESULT_GROUP&gt;
	&lt;BALANCE_INQUIRY_RESPONSES/&gt;
	&lt;EMBOSSER_INQUIRY_RESPONSES/&gt;
&lt;/RESULT_GROUP&gt;</expr:xqueryText></trf:expr></trf:assign>
        </bpel:extensionAssignOperation>
        <bpel:extensionAssignOperation>
        	<trf:assign varName="orgCount">
        		<trf:expr>
        			<expr:xqueryText>count($request.parameters/CARD_INFORMATION)</expr:xqueryText></trf:expr></trf:assign>
        </bpel:extensionAssignOperation></bpel:assign>
        <bpel:forEach parallel="no" counterName="orgPosition">
        	<bpel:startCounterValue>1</bpel:startCounterValue>
        	<bpel:finalCounterValue>$orgCount</bpel:finalCounterValue>
        	<bpel:scope>
        		<bpel:sequence>
        			
        			<bpel:assign>
        				
        				<bpel:extensionAssignOperation>
        					<bpel:documentation></bpel:documentation>
        					<trf:assign varName="cardNumber">
        						<trf:expr>
        							<expr:xqueryText>for $orgBase in $request.parameters/CARD_INFORMATION where $orgBase/ORG_TYPE/text() = &quot;BASE&quot;
return(
	string($orgBase/CARD_NUMBER/text())
)</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			<bpel:extensionAssignOperation>
		<bpel:documentation></bpel:documentation>
		<trf:assign varName="org">
			<trf:expr>
				<expr:xqueryText>string($request.parameters/CARD_INFORMATION[xs:int($orgPosition)]/ORG/text())</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="orgType">
        					<trf:expr>
        						<expr:xqueryText>string($request.parameters/CARD_INFORMATION[xs:int($orgPosition)]/ORG_TYPE/text())</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation></bpel:assign>
        			<bpel:if>
        				<bpel:condition>$cardNumber != '' and $org != '0'</bpel:condition>
        				<bpel:sequence>
        					<bpel:empty></bpel:empty>
        				<bpel:flow>
		<bpel:scope>
			<bpel:variables>
				<bpel:variable name="REQBalanceInquiry"
					messageType="ns1:BalanceInquiry_in">
				</bpel:variable>
				<bpel:variable name="RSPBalanceInquiry"
					messageType="ns1:BalanceInquiry_out">
				</bpel:variable></bpel:variables>
			<bpel:faultHandlers>
				<bpel:catchAll>
					<bpel:sequence>
						<bpel:empty></bpel:empty>
					<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:assign varName="response.result">
				<trf:expr>
					<expr:xqueryText>&lt;con:sjconsultaDatosTarjetaCreditoResponse&gt;
	&lt;ERROR_CODE&gt;ERROR&lt;/ERROR_CODE&gt;
	&lt;MENSAJE_ERROR&gt;Error al realizar la consulta en Vision plus: BalanceInquiry&lt;/MENSAJE_ERROR&gt;
&lt;/con:sjconsultaDatosTarjetaCreditoResponse&gt;</expr:xqueryText></trf:expr></trf:assign>
		</bpel:extensionAssignOperation>
					</bpel:assign></bpel:sequence>
				</bpel:catchAll></bpel:faultHandlers>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:assign varName="REQBalanceInquiry.BalanceInquiryRequest">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/Resources/VisionPlus/xq/balanceInquiryIn"></expr:resource>
									<expr:param name="accountNumber">
										<expr:path>$cardNumber</expr:path></expr:param>
									<expr:param name="org">
										<expr:path>$org</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
				</bpel:assign>
				<bpel:invoke operation="BalanceInquiry" partnerLink="vmx" inputVariable="REQBalanceInquiry" outputVariable="RSPBalanceInquiry">
					<rescon:invokeInfo>
						<rescon:service isProxy="false" ref="Middleware/v2/BusinessServices/VisionPlus/vmx/biz/vmx"></rescon:service></rescon:invokeInfo></bpel:invoke>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:insert varName="resultGroup">
							<trf:location>
								<expr:xpathText>./BALANCE_INQUIRY_RESPONSES</expr:xpathText></trf:location>
							<trf:where>last-child</trf:where>
						<trf:expr>
							<expr:xqueryText>$RSPBalanceInquiry.BalanceInquiryResponse</expr:xqueryText></trf:expr></trf:insert>
					</bpel:extensionAssignOperation>
				</bpel:assign></bpel:sequence>
		</bpel:scope>
		<bpel:scope>
			<bpel:variables>
				<bpel:variable name="REQEmbosserInquiry"
					messageType="ns1:EmbosserInquiry_in">
				</bpel:variable>
				<bpel:variable name="RSPEmbosserInquiry"
					messageType="ns1:EmbosserInquiry_out">
				</bpel:variable></bpel:variables>
			<bpel:faultHandlers>
				<bpel:catchAll>
					<bpel:sequence>
						<bpel:empty></bpel:empty>
					<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:assign varName="response.result">
				<trf:expr>
					<expr:xqueryText>&lt;con:sjconsultaDatosTarjetaCreditoResponse&gt;
	&lt;ERROR_CODE&gt;ERROR&lt;/ERROR_CODE&gt;
	&lt;MENSAJE_ERROR&gt;Error al realizar la consulta en Vision plus: EmbosserInquiry&lt;/MENSAJE_ERROR&gt;
&lt;/con:sjconsultaDatosTarjetaCreditoResponse&gt;</expr:xqueryText></trf:expr></trf:assign>
		</bpel:extensionAssignOperation>
					</bpel:assign></bpel:sequence>
				</bpel:catchAll></bpel:faultHandlers>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:assign varName="REQEmbosserInquiry.EmbosserInquiryRequest">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/Resources/VisionPlus/xq/embosserInquiryIn"></expr:resource>
									<expr:param name="cardNumber">
										<expr:path>$cardNumber</expr:path></expr:param>
									<expr:param name="org">
										<expr:path>$org</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
				</bpel:assign>
				<bpel:invoke operation="EmbosserInquiry" partnerLink="vmx" inputVariable="REQEmbosserInquiry" outputVariable="RSPEmbosserInquiry">
					<rescon:invokeInfo>
						<rescon:service isProxy="false" ref="Middleware/v2/BusinessServices/VisionPlus/vmx/biz/vmx"></rescon:service></rescon:invokeInfo></bpel:invoke>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:insert varName="resultGroup">
							<trf:location>
								<expr:xpathText>./EMBOSSER_INQUIRY_RESPONSES</expr:xpathText></trf:location>
							<trf:where>last-child</trf:where>
						<trf:expr>
							<expr:xqueryText>$RSPEmbosserInquiry.EmbosserInquiryResponse</expr:xqueryText></trf:expr></trf:insert>
					</bpel:extensionAssignOperation>
				</bpel:assign></bpel:sequence>
		</bpel:scope>
        				</bpel:flow></bpel:sequence>
        				<bpel:else>
        					<bpel:sequence>
        						<bpel:empty></bpel:empty>
        					</bpel:sequence>
        				</bpel:else>
        			</bpel:if></bpel:sequence>
        	</bpel:scope>
        </bpel:forEach>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.result">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/v3/BusinessServices/SJS/consultaDatosTarjetaCreditoNI/xq/sjConsultaDatosTarjetaCreditoOut"></expr:resource>
        					<expr:param name="sjconsultaDatosTarjetaCredito">
        						<expr:path>$request.parameters</expr:path></expr:param>
        					<expr:param name="resultGroup">
        						<expr:path>$resultGroup</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:reply partnerLink="sjConsultaDatosTarjetaCreditoNI" operation="sjconsultaDatosTarjetaCredito" variable="response"></bpel:reply>
    </bpel:sequence>
</bpel:process>