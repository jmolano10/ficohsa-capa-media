<?xml version="1.0" encoding="UTF-8"?>
<bpel:process name="sjConsultaDetalleRecaudoResiliencia"
              targetNamespace="http://www.example.com/flow/Middleware/v3/BusinessServices/Recaudos/flow/sjConsultaDetalleRecaudoResiliencia"
              xmlns:tns="http://www.example.com/flow/Middleware/v3/BusinessServices/Recaudos/flow/sjConsultaDetalleRecaudoResiliencia"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:rescon="http://www.bea.com/alsb/flow/resources/config"
              xmlns:bea="http://www.bea.com/bpel/ui/extensions"
              xmlns:ext="http://www.bea.com/bpel/extensions"
              xmlns:expr="http://www.bea.com/wli/sb/stages/config"
              xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config"
              xmlns:bind="T24WebServicesImpl" 
              bea:name="sjConsultaDetalleRecaudoResiliencia" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/InterbancaSOAApplication/ObtenerParametrizacion/obtenerParametrizacion" xmlns:osb="http://xmlns.oracle.com/pcbpel/adapter/db/ORA_BANK/OSB_GET_CONFIG/" xmlns:ns0="http://www.example.org/listadoErroresResilienciaPS/">
    <bpel:partnerLinks>
        <bpel:partnerLink name="sjConsultaDetalleRecaudoResiliencia" partnerLinkType="tns:sjConsultaDetalleRecaudoResiliencia"
                          myRole="sjConsultaDetalleRecaudoResiliencia">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="ObtenerParametrizacion"
		partnerLinkType="tns:ObtenerParametrizacion_plnkType"
		partnerRole="ObtenerParametrizacion_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="ListadoErroresResiliencia"
	   	partnerLinkType="tns:ListadoErroresResiliencia_plnkType"
	   	partnerRole="ListadoErroresResiliencia_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="RecaudosBS"
	   	partnerLinkType="tns:RecaudosBS_plnkType"
	   	partnerRole="RecaudosBS_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request"
	                      messageType="bind:sjConsultadedetallederecaudo">
	       </bpel:variable>
	       <bpel:variable name="response"
                       messageType="bind:sjConsultadedetallederecaudoResponse">
	       </bpel:variable>
    <bpel:variable name="retryCount" type="xsd:string"></bpel:variable>
    <bpel:variable name="terminarCiclo" type="xsd:boolean"></bpel:variable>
    <bpel:variable name="mensajesResiliencia"
    	element="ns0:listadoErroresResilienciaResponse">
    </bpel:variable>
    <bpel:variable name="REQUEST"
    	messageType="bind:Consultadedetallederecaudo">
    </bpel:variable>
    <bpel:variable name="RESPONSE"
    	messageType="bind:ConsultadedetallederecaudoResponse">
    </bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaDetalleRecaudoResiliencia" operation="Consultadedetallederecaudo" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                <rescon:wsdl ref="Middleware/v3/BusinessServices/Recaudos/wsdl/sjConsultaDetalleRecaudoResiliencia" binding="bind:T24WebServicesImplPortBinding"/>
            </rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="terminarCiclo">
        			<trf:expr>
        				<expr:xqueryText>0</expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:flow>
        	<bpel:scope bea:name="consultaParametrizacion">
        		<bpel:variables>
        			<bpel:variable name="ReqObtParam"
        				messageType="ns1:args_in_msg">
        			</bpel:variable>
        			<bpel:variable name="RspObtParam"
        				messageType="ns1:args_out_msg">
        			</bpel:variable></bpel:variables>
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="ReqObtParam.InputParameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/Business_Resources/general/Parametrizacion/obtenerParametrizacionIn"></expr:resource>
        								<expr:param name="nombreParametros">
        									<expr:path>&quot;RESILIENCIA.RETRYCOUNT&quot;</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign>
        			<bpel:invoke operation="obtenerParametrizacion" partnerLink="ObtenerParametrizacion" inputVariable="ReqObtParam" outputVariable="RspObtParam">
        				<rescon:invokeInfo>
        					<rescon:service isProxy="false" ref="Middleware/Business_Resources/general/Resources/ObtenerParametrizacion/ObtenerParametrizacion"></rescon:service></rescon:invokeInfo></bpel:invoke>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="retryCount">
        						<trf:expr>
        							<expr:xqueryText>let $parametro := fn:string(fn:number($RspObtParam.OutputParameters/osb:CONFIGURACIONES/osb:CONFIGURACIONES_ITEM[osb:NAME=&quot;RESILIENCIA.RETRYCOUNT&quot;]/osb:VALUE/text()))
return 
	if($parametro = &quot;NaN&quot;) then (
		&quot;0&quot;
	) else (
		$parametro
	)</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign></bpel:sequence>
        	</bpel:scope>
        	<bpel:scope bea:name="consultaMensajesResiliencia">
        		<bpel:variables>
        			<bpel:variable
        				name="listadoErroresResilienciaREQUEST"
        				messageType="ns0:listadoErroresResilienciaRequest">
        			</bpel:variable>
        			<bpel:variable
        				name="listadoErroresResilienciaRESPONSE"
        				messageType="ns0:listadoErroresResilienciaResponse">
        			</bpel:variable></bpel:variables>
        		<bpel:sequence>
        			<bpel:invoke operation="listadoErroresResiliencia" partnerLink="ListadoErroresResiliencia" inputVariable="listadoErroresResilienciaREQUEST" outputVariable="listadoErroresResilienciaRESPONSE">
        				<rescon:invokeInfo>
        					<rescon:service isProxy="true" ref="Middleware/v2/ProxyServices/ListadoErroresResiliencia"></rescon:service></rescon:invokeInfo></bpel:invoke>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="mensajesResiliencia">
        						<trf:expr>
        							<expr:xqueryText>$listadoErroresResilienciaRESPONSE.parameters</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign></bpel:sequence>
        	</bpel:scope>
        </bpel:flow>
        <bpel:repeatUntil>
        	<bpel:sequence>
        		<bpel:assign>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="retryCount">
        					<trf:expr>
        						<expr:xqueryText>number($retryCount) - 1</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        		<bpel:extensionAssignOperation>
		<trf:assign varName="REQUEST.parameters">
			<trf:expr>
				<expr:xqueryText>$request.parameters</expr:xqueryText></trf:expr></trf:assign>
        		</bpel:extensionAssignOperation></bpel:assign>
        		<bpel:invoke operation="Consultadedetallederecaudo" partnerLink="RecaudosBS" inputVariable="REQUEST" outputVariable="RESPONSE">
        			<rescon:invokeInfo>
        				<rescon:service isProxy="false" ref="Middleware/Business_Resources/recaudos/Resources/RecaudosBS"></rescon:service></rescon:invokeInfo></bpel:invoke>
        		<bpel:assign>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="terminarCiclo">
        					<trf:expr>
        						<expr:xqueryText>not(fn:exists($mensajesResiliencia/MENSAJES_ERRORES[MENSAJE_ERROR = $RESPONSE.parameters/Status/messages]))</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        		</bpel:assign></bpel:sequence>
        	<bpel:condition>string($RESPONSE.parameters/Status/successIndicator/text()) = &quot;Success&quot; or number($retryCount) &lt; 0 or $terminarCiclo</bpel:condition>
        </bpel:repeatUntil>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.parameters">
        			<trf:expr>
        				<expr:xqueryText>$RESPONSE.parameters</expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:reply partnerLink="sjConsultaDetalleRecaudoResiliencia" operation="Consultadedetallederecaudo" variable="response"></bpel:reply>
    </bpel:sequence>
</bpel:process>