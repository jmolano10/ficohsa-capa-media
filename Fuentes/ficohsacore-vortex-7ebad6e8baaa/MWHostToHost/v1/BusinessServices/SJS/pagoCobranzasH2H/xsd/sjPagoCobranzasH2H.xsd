<xs:schema targetNamespace="http://www.ficohsa.com.hn/middleware.services/sjCobranzasH2HTypes" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:hcob="http://www.ficohsa.com.hn/middleware.services/sjCobranzasH2HTypes" xmlns:gen="http://www.ficohsa.com.hn/middleware.services/genericTypes">

    <xs:import namespace="http://www.ficohsa.com.hn/middleware.services/genericTypes" schemaLocation="../../../../Resources/Generales/xsd/genericTypes.xsd"/>

	<!-- INFO TYPE -->
	<xs:complexType name="infoType">
		<xs:sequence>
			<xs:element name="FIELD_NAME" type="xs:string"/>
			<xs:element name="FIELD_VALUE" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<!--############################### -->
	<!-- OPERACION: PAGO COBRANZAS -->
	<!--############################### -->
	<xs:element name="sjPagoCobranzas">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="USER_NAME" type="gen:stringMinLength1"/>
            	<xs:element name="PASSWORD" type="gen:stringMinLength1"/>
            	<xs:element name="SOURCE_BANK" type="gen:stringMinLength1"/>
				<xs:element name="CUSTOMER_ID" type="gen:stringMinLength1"/>
				<xs:element name="SERVICES">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SERVICE" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ID" type="gen:greaterThan0"/>
										<xs:element name="CONTRACT_ID" type="xs:string"/>
										<xs:element name="NAME" type="xs:string"/>
										<xs:element name="ACCOUNT" minOccurs="0"/>											
										<xs:element name="PAYMENT_INFORMATION">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="PYMT_METHOD">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="ACCOUNT_DEBIT"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="PYMT_ID" type="gen:stringMinLength1"/>
							                        <xs:element name="PYMT_CURRENCY" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="DEBTORS">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="DEBTOR" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="TRACE" type="xs:string" minOccurs="0"/>
																<xs:element name="CODE" type="gen:stringMinLength1"/>
																<xs:element name="AMOUNT" type="gen:greaterThan0"/>
																<xs:element name="EXTENDED_COLLECTION" type="gen:yesNoType"/>
																<xs:element name="ADDITIONAL_INFO" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="INFO" type="hcob:infoType" maxOccurs="unbounded"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="sjPagoCobranzasResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SUCCESS_INDICATOR" type="xs:string" minOccurs="0"/>
				<xs:element name="SUCCESS_AMOUNT" type="xs:string" minOccurs="0"/>
				<xs:element name="ERROR_AMOUNT" type="xs:string" minOccurs="0"/>
				<xs:element name="SERVICES" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SERVICE" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="SUCCESS_INDICATOR" type="xs:string" minOccurs="0"/>
									<xs:element name="ERROR_MESSAGE" type="xs:string" minOccurs="0"/>
									<xs:element name="ID" type="xs:integer" minOccurs="0"/>
									<xs:element name="NAME" type="xs:string" minOccurs="0"/>
									<xs:element name="DEBTORS" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="DEBTOR" minOccurs="0" maxOccurs="unbounded">
												<xs:complexType>
													<xs:sequence>
											            <xs:element name="TRANSACTION_ID" type="xs:string" minOccurs="0"/>
														<xs:element name="SUCCESS_INDICATOR" type="xs:string" minOccurs="0"/>
														<xs:element name="ERROR_MESSAGE" type="xs:string" minOccurs="0"/>
														<xs:element name="TRACE" type="xs:string" minOccurs="0"/>
														<xs:element name="CODE" type="xs:string" minOccurs="0"/>
														<xs:element name="NAME" type="xs:string" minOccurs="0"/>
														<xs:element name="PAYMENT_INFORMATION" minOccurs="0">
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="PYMT_METHOD" type="xs:string" minOccurs="0"/>
																	<xs:element name="CURRENCY" type="gen:currencyType" minOccurs="0"/>
																	<xs:element name="AMOUNT" type="xs:decimal" minOccurs="0"/>
																	<xs:element name="LATE_FEE" type="xs:decimal" minOccurs="0"/>
																	<xs:element name="TOTAL" type="xs:decimal" minOccurs="0"/>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="DEALSLIP" type="xs:string" minOccurs="0"/>
													</xs:sequence>
												</xs:complexType>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>	

</xs:schema>