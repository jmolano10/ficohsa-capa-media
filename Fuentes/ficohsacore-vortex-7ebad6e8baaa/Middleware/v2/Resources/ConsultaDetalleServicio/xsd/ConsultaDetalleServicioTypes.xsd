<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:FicCpa="http://www.ficohsa.com.hn/middleware.services/ConsultaDetalleServicioTypes" targetNamespace="http://www.ficohsa.com.hn/middleware.services/ConsultaDetalleServicioTypes" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xs:element name="consultaDetalleServicio" type="FicCpa:ConsultaDetalleServicioRequestIM"/>
	<xs:element name="consultaDetalleServicioResponse" type="FicCpa:ConsultaDetalleServicioResponseIM"/>
	<xs:complexType name="ConsultaDetalleServicioRequestIM">
		<xs:sequence>
			<xs:element name="SERVICE_ID" type="xs:string"/>
			<xs:element name="CHANNEL" type="FicCpa:ChannelTypeEnum"/>
			<xs:element name="TYPE" type="FicCpa:TypeTypeEnum"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConsultaDetalleServicioResponseIM">
		<xs:sequence>
			<xs:element name="SERVICE_INFO" type="FicCpa:ServiceInfoType"  minOccurs="0"/>
			<xs:element name="SERVICE_DETAILS" type="FicCpa:ServiceDetailsType"  minOccurs="0"/>
			<xs:element name="INFO_ADITIONAL_SERVICE" type="FicCpa:InfoAditionalServiceType"  minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceInfoType">
		<xs:sequence>
			<xs:element name="ID" type="xs:string"/>
			<xs:element name="CONTRACT_ID" type="xs:string"/>
			<xs:element name="TYPE" type="xs:string"/>
			<xs:element name="IS_DATA_CONFIG" type="xs:string"/>
			<xs:element name="DESCRIPTION" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceDetailsType">
		<xs:sequence>
			<xs:element name="SERVICE_FIELDS" type="FicCpa:ServiceFieldsType" minOccurs="0"/>
			<xs:element name="SERVICE_DATA_INPUT_CONFIG" type="FicCpa:ServiceDataInputConfigType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceFieldsType">
		<xs:sequence>
			<xs:element name="SERVICE_FIELD" type="FicCpa:ServiceFieldType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
		<xs:complexType name="ServiceDataInputConfigType">
		<xs:sequence>
			<xs:element name="SERVICE_DATA" type="FicCpa:ServiceDataInputConfigFieldType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceFieldType">
		<xs:sequence>
			<xs:element name="NAME" type="xs:string"/>
			<xs:element name="DESCRIPTION" type="xs:string"/>
			<xs:element name="ENABLE_CATALOG" type="xs:string"/>
			<xs:element name="CATALOG_ITEMS" type="FicCpa:CatalogItemsType"  minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ServiceDataInputConfigFieldType">
		<xs:sequence>
			<xs:element name="NAME" type="xs:string" minOccurs="0"/>
			<xs:element name="VALUE" type="xs:string" minOccurs="0"/>
			<xs:element name="ENABLE_CATALOG" type="xs:string"/>
			<xs:element name="CATALOG_ITEMS" type="FicCpa:CatalogItemsType"  minOccurs="0"/>
