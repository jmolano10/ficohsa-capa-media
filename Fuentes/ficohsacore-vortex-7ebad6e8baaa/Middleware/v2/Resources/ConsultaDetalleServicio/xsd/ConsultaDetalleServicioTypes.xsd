			<xs:element name="VALUE" type="xs:string"  minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InfoAditionalServiceType">
		<xs:sequence>
			<xs:element name="AVALIABLE_PRODUCTS_PAYMENT" type="FicCpa:AvailableProductsPaymentType"/>
			<xs:element name="ALLOW_PARTIAL_PAYMENT" type="xs:string"/>
			<xs:element name="ENABLE_CROSSING_CURRENCY" type="xs:string"/>
