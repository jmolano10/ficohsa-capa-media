<bpel:process name="sjConsultaRemesaResiliencia" targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/Remesas/flow/sjConsultaRemesaResiliencia" bea:name="sjConsultaRemesaResiliencia" xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/Remesas/flow/sjConsultaRemesaResiliencia" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:rescon="http://www.bea.com/alsb/flow/resources/config" xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions" xmlns:expr="http://www.bea.com/wli/sb/stages/config" xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config" xmlns:bind="T24WebServicesImpl" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/InterbancaSOAApplication/ObtenerParametrizacion/obtenerParametrizacion" xmlns:osb="http://xmlns.oracle.com/pcbpel/adapter/db/ORA_BANK/OSB_GET_CONFIG/" xmlns:ns0="http://www.example.org/listadoErroresResilienciaPS/">
    <bpel:partnerLinks>
        <bpel:partnerLink name="sjConsultaRemesaResiliencia" partnerLinkType="tns:sjConsultaRemesaResiliencia" myRole="sjConsultaRemesaResiliencia">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="ObtenerParametrizacion" partnerLinkType="tns:ObtenerParametrizacion_plnkType" partnerRole="ObtenerParametrizacion_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="ListadoErroresResiliencia" partnerLinkType="tns:ListadoErroresResiliencia_plnkType" partnerRole="ListadoErroresResiliencia_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="Remesas" partnerLinkType="tns:Remesas_plnkType" partnerRole="Remesas_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request" messageType="bind:sjConsultaderemesa">
	       </bpel:variable>
	       <bpel:variable name="response" messageType="bind:sjConsultaderemesaResponse">
	       </bpel:variable>
    <bpel:variable name="terminarCiclo" type="xsd:boolean"/>
    <bpel:variable name="retryCount" type="xsd:string"/>
    <bpel:variable name="mensajesResiliencia" element="ns0:listadoErroresResilienciaResponse">
    </bpel:variable>
    <bpel:variable name="REQConsultaRemT24" messageType="bind:Consultaderemesa">
    </bpel:variable>
    <bpel:variable name="RSPConsultaRemT24" messageType="bind:ConsultaderemesaResponse">
    </bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaRemesaResiliencia" operation="Consultaderemesa" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                
            <rescon:wsdl ref="Middleware/v2/BusinessServices/Remesas/wsdl/sjConsultaRemesaResiliencia" binding="bind:T24WebServicesImplPortBinding"/></rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="terminarCiclo">
        			<trf:expr>
        				<expr:xqueryText>0</expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:flow>
        	<bpel:scope bea:name="consultaParametrizacion">
        		<bpel:variables>
        			<bpel:variable name="ReqObtParam" messageType="ns1:args_in_msg">
        			</bpel:variable>
        			<bpel:variable name="RspObtParam" messageType="ns1:args_out_msg">
        			</bpel:variable></bpel:variables>
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="ReqObtParam.InputParameters">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/Business_Resources/general/Parametrizacion/obtenerParametrizacionIn"/>
        								<expr:param name="nombreParametros">
        									<expr:path>"RESILIENCIA.RETRYCOUNT"</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign>
        			<bpel:invoke operation="obtenerParametrizacion" partnerLink="ObtenerParametrizacion" inputVariable="ReqObtParam" outputVariable="RspObtParam">
        				<rescon:invokeInfo><rescon:service ref="Middleware/Business_Resources/general/Resources/ObtenerParametrizacion/ObtenerParametrizacion" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="retryCount">
        						<trf:expr>
        							<expr:xqueryText>let $parametro := fn:string(fn:number($RspObtParam.OutputParameters/osb:CONFIGURACIONES/osb:CONFIGURACIONES_ITEM[osb:NAME="RESILIENCIA.RETRYCOUNT"]/osb:VALUE/text()))
return 
	if($parametro = "NaN") then (
		"0"
	) else (
		$parametro
	)</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign></bpel:sequence>
        	</bpel:scope>
        	<bpel:scope bea:name="consultaMensajesResiliencia">
        		<bpel:variables>
        			<bpel:variable name="listadoErroresResilienciaREQUEST" messageType="ns0:listadoErroresResilienciaRequest">
        			</bpel:variable>
        			<bpel:variable name="listadoErroresResilienciaRESPONSE" messageType="ns0:listadoErroresResilienciaResponse">
        			</bpel:variable></bpel:variables>
        		<bpel:sequence>
        			<bpel:invoke operation="listadoErroresResiliencia" partnerLink="ListadoErroresResiliencia" inputVariable="listadoErroresResilienciaREQUEST" outputVariable="listadoErroresResilienciaRESPONSE">
        				<rescon:invokeInfo><rescon:service ref="Middleware/v2/ProxyServices/ListadoErroresResiliencia" xsi:type="ref:ProxyRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:assign varName="mensajesResiliencia">
        						<trf:expr>
        							<expr:xqueryText>$listadoErroresResilienciaRESPONSE.parameters</expr:xqueryText></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign></bpel:sequence>
        	</bpel:scope>
        </bpel:flow>
        <bpel:repeatUntil>
        	<bpel:sequence>
        		<bpel:assign>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="retryCount">
        					<trf:expr>
        						<expr:xqueryText>number($retryCount) - 1</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        		<bpel:extensionAssignOperation>
		<trf:assign varName="REQConsultaRemT24.parameters">
			<trf:expr>
				<expr:xqueryText>$request.parameters</expr:xqueryText></trf:expr></trf:assign>
        		</bpel:extensionAssignOperation></bpel:assign>
        		<bpel:invoke operation="Consultaderemesa" partnerLink="Remesas" inputVariable="REQConsultaRemT24" outputVariable="RSPConsultaRemT24">
        			<rescon:invokeInfo><rescon:service ref="Middleware/Business_Resources/remesas/Resoruces/Remesas" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
        		<bpel:assign>
        			<bpel:extensionAssignOperation>
        				<trf:assign varName="terminarCiclo">
        					<trf:expr>
        						<expr:xqueryText>not(fn:exists($mensajesResiliencia/MENSAJES_ERRORES[MENSAJE_ERROR = $RSPConsultaRemT24.parameters/Status/messages]))</expr:xqueryText></trf:expr></trf:assign>
        			</bpel:extensionAssignOperation>
        		</bpel:assign></bpel:sequence>
        	<bpel:condition>string($RSPConsultaRemT24.parameters/Status/successIndicator/text()) = "Success" or number($retryCount) &lt; 0 or $terminarCiclo</bpel:condition>
        </bpel:repeatUntil>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.parameters">
        			<trf:expr>
        				<expr:xqueryText>$RSPConsultaRemT24.parameters</expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:reply partnerLink="sjConsultaRemesaResiliencia" operation="Consultaderemesa" variable="response"/>
    </bpel:sequence>
</bpel:process>