<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="CloudCodeLoginRestBS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.ficohsa.com.hn/honduras/cloudCodeLoginTypes" schemaLocation="../Schemas/CloudCodeLoginTypes.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/auth/login">
         <method name="POST" soa:wsdlOperation="login">
            <request>
               <representation mediaType="application/json" element="cns:cloudCodeLogin" xmlns:cns="http://www.ficohsa.com.hn/honduras/cloudCodeLoginTypes"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:cloudCodeLoginResponse" xmlns:cns="http://www.ficohsa.com.hn/honduras/cloudCodeLoginTypes"/>
            </response>
         </method>
      </resource>
   </resources>
</application>