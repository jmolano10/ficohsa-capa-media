<bpel:process name="ConsultaTCClienteGT" targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/flow/ConsultaTCClienteGT" bea:name="ConsultaTCClienteGT" xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/flow/ConsultaTCClienteGT" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:rescon="http://www.bea.com/alsb/flow/resources/config" xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions" xmlns:expr="http://www.bea.com/wli/sb/stages/config" xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config" xmlns:bind="ConsultaTarjetaCreditoCliente" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.ficohsa.com.hn/middleware.services/consultaTarjetasCreditoPorCliente/" xmlns:ns2="http://www.ficohsa.com.hn/middleware.services/consultaSaldosTarjetaCreditoPS/" xmlns:con1="http://www.ficohsa.com.hn/middleware.services/consultaTarjetasCreditoPorClienteTypes">
    <bpel:partnerLinks>
        <bpel:partnerLink name="ConsultaTCClienteGT" partnerLinkType="tns:ConsultaTCClienteGT" myRole="ConsultaTCClienteGT">
	       </bpel:partnerLink>
	   
	   <bpel:partnerLink name="ConsultaTarjetasCreditoPorCliente" partnerLinkType="tns:ConsultaTarjetasCreditoPorCliente_plnkType" partnerRole="ConsultaTarjetasCreditoPorCliente_PartnerRole">
	   </bpel:partnerLink>
	   <bpel:partnerLink name="ConsultaSaldosTarjetaCredito" partnerLinkType="tns:ConsultaSaldosTarjetaCredito_plnkType" partnerRole="ConsultaSaldosTarjetaCredito_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request" messageType="bind:ConsultaTarjetaCreditoClienteRequest">
	       </bpel:variable>
	       <bpel:variable name="response" messageType="bind:ConsultaTarjetaCreditoClienteResponse">
	       </bpel:variable>
    
    
    
    
    
    
    
    <bpel:variable name="REQConsultaTarjetasCreditoPorCliente" messageType="ns0:consultaTarjetasCreditoPorClienteRequest">
    </bpel:variable>
    <bpel:variable name="RSPConsultaTarjetasCreditoPorCliente" messageType="ns0:consultaTarjetasCreditoPorClienteResponse">
    </bpel:variable>
    </bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="ConsultaTCClienteGT" operation="ConsultaTarjetaCreditoCliente" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                
            <rescon:wsdl ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/ConsultaTarjetaCreditoCliente" binding="bind:ConsultaTarjetaCreditoClienteBinding"/></rescon:receiveInfo>
        </bpel:receive>
        
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<bpel:documentation/>
        		<trf:assign varName="REQConsultaTarjetasCreditoPorCliente.parameters">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/xq/sjConsultaTarjetasCreditoPorClienteIn"/>
        					
        					
        					
        					
        					<expr:param name="customerIdType">
        						<expr:path>"CUSTOMER_ID"</expr:path></expr:param>
        					<expr:param name="creditCardStatus">
        						<expr:path>"1"</expr:path></expr:param>
        					<expr:param name="customerIdValue">
        						<expr:path>fn:string($request.inputParameters/bind:PARAMETERS/bind:CUSTOMERID/text())</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation>
        	<bpel:extensionAssignOperation>
        		<bpel:documentation/>
        		<trf:assign varName="REQConsultaTarjetasCreditoPorCliente.RequestHeader">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/xq/sjConsultaTCClienteGTHeaderIn"/>
        					<expr:param name="region">
        						<expr:path>"GT01"</expr:path></expr:param>
        					<expr:param name="consultaTarjetaCreditoTypeRequest">
        						<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        <bpel:invoke operation="consultaTarjetasCreditoPorCliente" partnerLink="ConsultaTarjetasCreditoPorCliente" outputVariable="RSPConsultaTarjetasCreditoPorCliente" inputVariable="REQConsultaTarjetasCreditoPorCliente">
        	<rescon:invokeInfo><rescon:service ref="Middleware/v2/ProxyServices/ConsultaTarjetasCreditoPorCliente" xsi:type="ref:ProxyRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
        
        
        
        
        
        
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.outputParameters">
        			<trf:expr>
        				<expr:xqueryText>&lt;bind:ConsultaTarjetaCreditoTypeResponse>
	&lt;bind:CREDIT_CARD_DETAILS>
	&lt;/bind:CREDIT_CARD_DETAILS>
&lt;/bind:ConsultaTarjetaCreditoTypeResponse></expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        
        
        
        
        
        <bpel:if>
        	<bpel:condition>string($RSPConsultaTarjetasCreditoPorCliente.ResponseHeader/successIndicator/text()) = 'Success'</bpel:condition>
        	<bpel:sequence>
        		<bpel:empty/>
        	<bpel:forEach parallel="yes" counterName="counter">
		<bpel:startCounterValue>number(1)</bpel:startCounterValue>
		<bpel:finalCounterValue>count($RSPConsultaTarjetasCreditoPorCliente.result/CREDIT_CARDS/CREDIT_CARD_INFORMATION)</bpel:finalCounterValue>
		<bpel:scope>
			<bpel:variables>
				
				
				<bpel:variable type="xsd:string" name="cardStatus"/>
				<bpel:variable name="REQConsultaSaldosTarjetaCredito" messageType="ns2:consultaSaldosTarjetaCreditoRequest">
				</bpel:variable>
				<bpel:variable name="RSPConsultaSaldosTarjetaCredito" messageType="ns2:consultaSaldosTarjetaCreditoResponse">
				</bpel:variable></bpel:variables>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<bpel:documentation/>
						<trf:assign varName="REQConsultaSaldosTarjetaCredito.parameters">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/xq/sjConsultaSaldosTarjetaCreditoIn"/>
									
									
									
									
									
									
									
									
									
									<expr:param name="cardNumber">
	<expr:path>string($RSPConsultaTarjetasCreditoPorCliente.result/CREDIT_CARDS/CREDIT_CARD_INFORMATION[xs:int($counter)]/CREDIT_CARD_NUMBER/text())</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
					<bpel:extensionAssignOperation>
						<trf:assign varName="REQConsultaSaldosTarjetaCredito.RequestHeader">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/xq/sjConsultaTCClienteGTHeaderIn"/>
									
									
									
									
									<expr:param name="region">
										<expr:path>"GT01"</expr:path></expr:param>
									<expr:param name="consultaTarjetaCreditoTypeRequest">
										<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
					</bpel:extensionAssignOperation>
					
					
					<bpel:extensionAssignOperation>
						<trf:assign varName="cardStatus">
							<trf:expr>
								<expr:xqueryText>string($RSPConsultaTarjetasCreditoPorCliente.result/CREDIT_CARDS/CREDIT_CARD_INFORMATION[xs:int($counter)]/STATUS/text())</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
				<bpel:if>
					<bpel:condition>$cardStatus = "20" </bpel:condition>
					<bpel:sequence>
						<bpel:empty/>
						<bpel:invoke partnerLink="ConsultaSaldosTarjetaCredito" operation="consultaSaldosTarjetaCredito" inputVariable="REQConsultaSaldosTarjetaCredito" outputVariable="RSPConsultaSaldosTarjetaCredito">
							<rescon:invokeInfo><rescon:service ref="Middleware/v2/ProxyServices/ConsultaSaldosTarjetaCredito" xsi:type="ref:ProxyRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
						<bpel:if>
							<bpel:condition>$RSPConsultaSaldosTarjetaCredito.ResponseHeader/successIndicator = 'Success'</bpel:condition>
							<bpel:sequence>
								<bpel:empty/>
							<bpel:assign>
		<bpel:extensionAssignOperation>
			<bpel:documentation/>
			<trf:insert varName="response.outputParameters">
				<trf:location>
					<expr:xpathText>./bind:CREDIT_CARD_DETAILS</expr:xpathText></trf:location>
				<trf:where>last-child</trf:where>
				<trf:expr>
					<expr:xqueryTransform>
						<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/xq/sjDatosTarjetaOut"/>
						
						
						
						
						
						
						
						<expr:param name="consultaSaldosTarjetaCreditoResponse">
							<expr:path>$RSPConsultaSaldosTarjetaCredito.result</expr:path></expr:param>
						<expr:param name="cardNumber">
							<expr:path>string($RSPConsultaTarjetasCreditoPorCliente.result/CREDIT_CARDS/CREDIT_CARD_INFORMATION[xs:int($counter)]/CREDIT_CARD_NUMBER/text())</expr:path></expr:param>
						<expr:param name="successIndicator">
							<expr:path>string($RSPConsultaSaldosTarjetaCredito.ResponseHeader/successIndicator/text())</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
							<bpel:else>
								<bpel:sequence>
									<bpel:empty/>
								</bpel:sequence>
							</bpel:else>
						</bpel:if>
						</bpel:sequence>
					<bpel:else>
						<bpel:sequence>
							<bpel:empty/></bpel:sequence></bpel:else></bpel:if></bpel:sequence></bpel:scope></bpel:forEach>
        	<bpel:if>
        		<bpel:condition>count($response.outputParameters/bind:CREDIT_CARD_DETAILS/bind:CREDIT_CARD_DETAIL) > 0</bpel:condition>
        		<bpel:sequence>
        			<bpel:empty/>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:insert varName="response.outputParameters">
        						<trf:where>first-child</trf:where>
        						<trf:expr>
        							<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>SUCCESS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        		<bpel:else>
        			<bpel:sequence>
        				<bpel:empty/>
        				<bpel:assign>
        					<bpel:extensionAssignOperation>
        						<trf:insert varName="response.outputParameters">
        							<trf:where>first-child</trf:where>
        							<trf:expr>
        								<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>ERROR&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence></bpel:else></bpel:if></bpel:sequence>
        	<bpel:elseif>
        		<bpel:condition>string($RSPConsultaTarjetasCreditoPorCliente.ResponseHeader/successIndicator/text()) = 'NO RECRODS'</bpel:condition>
        		<bpel:sequence>
        			<bpel:empty/>
        		<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
			<trf:expr>
				<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>NO RECORDS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert>
		</bpel:extensionAssignOperation>
        		</bpel:assign></bpel:sequence>
        	</bpel:elseif>
        	<bpel:else>
        		<bpel:sequence>
        			<bpel:empty/>
        		<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>{ string($RSPConsultaTarjetasCreditoPorCliente.ResponseHeader/successIndicator/text())  }&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        	</bpel:else>
        </bpel:if>
        
        
        
        <bpel:reply partnerLink="ConsultaTCClienteGT" operation="ConsultaTarjetaCreditoCliente" variable="response"/>
    </bpel:sequence>
</bpel:process>