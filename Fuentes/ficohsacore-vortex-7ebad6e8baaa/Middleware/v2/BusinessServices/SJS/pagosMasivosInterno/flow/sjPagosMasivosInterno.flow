        																<expr:xqueryText>fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/gWSACCOUNTLISTDetailType/mWSACCOUNTLISTDetailType/INACTIVEMARKER/text())="Y"</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
        													<bpel:extensionAssignOperation>
