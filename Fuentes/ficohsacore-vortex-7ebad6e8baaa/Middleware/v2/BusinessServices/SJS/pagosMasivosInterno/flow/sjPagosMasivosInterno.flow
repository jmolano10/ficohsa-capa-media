or
fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/gWSACCOUNTLISTDetailType/mWSACCOUNTLISTDetailType/POSTINGRESTRICT/text()) = "ALL"</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        												<bpel:if>
