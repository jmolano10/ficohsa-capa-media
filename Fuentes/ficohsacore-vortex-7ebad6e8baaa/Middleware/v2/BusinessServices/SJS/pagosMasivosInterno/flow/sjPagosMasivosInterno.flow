			) else()
		) else (
			<ERROR_CODE>{ $successIndicator }</ERROR_CODE>,
			for $messages in $RSPconsultadedetalledelacuentaResponse.parameters/Status/messages
			return
			<ERROR_MESSAGE>{ data($messages) }</ERROR_MESSAGE>
