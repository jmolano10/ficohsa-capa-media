fn:string($RSPconsultadedetalledelacuentaResponse.parameters/WSACCOUNTLISTType[1]/ZERORECORDS/text()) = ""</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
