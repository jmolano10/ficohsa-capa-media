        									<expr:path>$request.inputParameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        				</bpel:extensionAssignOperation>
        			</bpel:assign>
        			<bpel:invoke operation="OpConsultaTarjetaDebito" partnerLink="ConsultaDatosTarjetaDebito" inputVariable="REQOpConsultaTarjetaDebito" outputVariable="RSPOpConsultaTarjetaDebito">
        				<rescon:invokeInfo><rescon:service ref="Middleware/v2/BusinessServices/CTS/ConsultaDatosTarjetaDebito/biz/ConsultaDatosTarjetaDebito" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke></bpel:sequence>
        	</bpel:scope>
        	<bpel:scope bea:name="BinesTC">
        		
        		
        		<bpel:sequence>
        			<bpel:assign>
        				
        				
        				
        				<bpel:extensionAssignOperation bea:name="codigoPais">
        					<trf:assign varName="codigoPais">
        						<trf:expr>
        							<expr:xqueryTransform>
        								<expr:resource ref="Middleware/v2/Resources/Generales/xq/convertirCodigoPais"/>
        								<expr:param name="sentidoConversion">
        									<expr:path>'OSB-ISO3'</expr:path></expr:param>
        								<expr:param name="codigoPais">
        									<expr:path>'NI01'</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        			<bpel:if>
        				<bpel:condition>$request.inputParameters/sjc:OPERATION = 'consultaPuntosLealtad' or 
$request.inputParameters/sjc:OPERATION = 'redencionPuntosLealtad'</bpel:condition>
        				<bpel:sequence>
        					<bpel:empty/>
        				
        				<bpel:assign>
        					<bpel:extensionAssignOperation bea:name="REQConDatoCuenta">
        						<trf:assign varName="REQConDatoCuenta.InputParameters">
        							<trf:expr>
        								<expr:xqueryTransform>
        									<expr:resource ref="Middleware/v2/Resources/MasterData/xq/conDatoCuentaIn"/>
        									
        									
        									
        									
        									<expr:param name="accountNumber">
        										<expr:path>fn:string($request.inputParameters/sjc:CARD_NUMBER/text())</expr:path></expr:param>
        									<expr:param name="codigoPais">
        										<expr:path>$codigoPais</expr:path></expr:param>
        									<expr:param name="org">
        										<expr:path>''</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation></bpel:assign>
        				<bpel:invoke inputVariable="REQConDatoCuenta" partnerLink="conDatoCuenta_db" outputVariable="RSPConDatoCuenta" operation="conDatoCuenta">
        					<rescon:invokeInfo><rescon:service ref="Middleware/v2/BusinessServices/MasterData/conDatoCuenta/biz/conDatoCuenta_db" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke></bpel:sequence>
        				<bpel:else>
        					<bpel:sequence>
        						<bpel:empty/>
        					
        					<bpel:assign>
        						<bpel:extensionAssignOperation bea:name="REQConDatoTarjeta">
        							<trf:assign varName="REQConDatoTarjeta.InputParameters">
        								<trf:expr>
        									<expr:xqueryTransform>
        										<expr:resource ref="Middleware/v2/Resources/MasterData/xq/conDatoTarjetaIn"/>
        										
        										
        										
        										
        										
        										
        										
        										
        										
        										
        										<expr:param name="cardNumber">
        											<expr:path>fn:string($request.inputParameters/sjc:CARD_NUMBER/text())</expr:path></expr:param>
        										<expr:param name="codigoPais">
        											<expr:path>$codigoPais</expr:path></expr:param>
        										<expr:param name="org">
        											<expr:path>''</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        						</bpel:extensionAssignOperation>
        					</bpel:assign>
        					<bpel:invoke operation="conDatoTarjeta" partnerLink="conDatoTarjeta_db" inputVariable="REQConDatoTarjeta" outputVariable="RSPConDatoTarjeta">
        						<rescon:invokeInfo><rescon:service ref="Middleware/v2/BusinessServices/MasterData/conDatoTarjeta/biz/conDatoTarjeta_db" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke></bpel:sequence>
        				</bpel:else>
        			</bpel:if>
        			
        			
        			
        			</bpel:sequence>
        	</bpel:scope>
        </bpel:flow>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
