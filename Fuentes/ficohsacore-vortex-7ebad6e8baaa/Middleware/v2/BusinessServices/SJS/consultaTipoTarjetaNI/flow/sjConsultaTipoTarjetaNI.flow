<bpel:process name="sjConsultaTipoTarjetaNI" targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTipoTarjetaNI/flow/sjConsultaTipoTarjetaNI" bea:name="sjConsultaTipoTarjetaNI" xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTipoTarjetaNI/flow/sjConsultaTipoTarjetaNI" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:rescon="http://www.bea.com/alsb/flow/resources/config" xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions" xmlns:expr="http://www.bea.com/wli/sb/stages/config" xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config" xmlns:bind="http://www.ficohsa.com.hn/middleware.services/sjConsultaTipoTarjetaNIPS" xmlns:ns0="http://www.ficohsa.com.hn/middleware.services/sjConsultaTipoTarjetaNIPS" xmlns:ns2="http://impl.service.srvaplcobistarjetadebito.ecobis.cobiscorp/" xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/db/Application2/Project1/conDatoCuenta" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/db/Application2/Project1/conDatoTarjeta" xmlns:sjc="http://www.ficohsa.com.hn/middleware.services/sjConsultaTipoTarjetaNI">
    <bpel:partnerLinks>
