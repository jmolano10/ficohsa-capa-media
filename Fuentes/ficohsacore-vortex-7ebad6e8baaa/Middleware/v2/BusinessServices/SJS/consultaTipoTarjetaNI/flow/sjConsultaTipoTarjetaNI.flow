        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:reply partnerLink="sjConsultaTipoTarjetaNI" operation="ConsultaTipoTarjeta" variable="response"/>
    </bpel:sequence>
</bpel:process>