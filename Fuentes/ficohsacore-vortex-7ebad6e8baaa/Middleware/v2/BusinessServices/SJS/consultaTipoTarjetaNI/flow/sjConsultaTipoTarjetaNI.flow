    <bpel:sequence>
        <bpel:receive partnerLink="sjConsultaTipoTarjetaNI" operation="ConsultaTipoTarjeta" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                
            <rescon:wsdl ref="Middleware/v2/BusinessServices/SJS/consultaTipoTarjetaNI/wsdl/sjConsultaTipoTarjetaNI" binding="bind:sjConsultaTipoTarjetaNIPSSOAP"/></rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<bpel:documentation/>
        		<trf:assign varName="RSPConDatoCuenta.OutputParameters">
        			<trf:expr>
        				<expr:xqueryText>&lt;con1:OutputParameters xmlns:con1='http://xmlns.oracle.com/pcbpel/adapter/db/sp/conDatoCuenta'/></expr:xqueryText></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        <bpel:extensionAssignOperation>
		<bpel:documentation/>
		<trf:assign varName="RSPConDatoTarjeta.OutputParameters">
			<trf:expr>
				<expr:xqueryText>&lt;con2:OutputParameters xmlns:con2='http://xmlns.oracle.com/pcbpel/adapter/db/sp/conDatoTarjeta'/></expr:xqueryText></trf:expr></trf:assign>
        </bpel:extensionAssignOperation></bpel:assign>
        
        <bpel:flow>
        	<bpel:scope bea:name="BinesTD">
        		<bpel:sequence>
        			<bpel:assign>
        				<bpel:extensionAssignOperation bea:name="REQOpConsultaTarjetaDebito">
        					<trf:assign varName="REQOpConsultaTarjetaDebito.parameters">
