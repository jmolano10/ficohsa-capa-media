<bpel:process name="ConsultaTCClienteGT" targetNamespace="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/flow/ConsultaTCClienteGT" bea:name="ConsultaTCClienteGT" xmlns:tns="http://www.example.com/flow/Middleware/v2/BusinessServices/SJS/consultaTCClienteGT/flow/ConsultaTCClienteGT" xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:rescon="http://www.bea.com/alsb/flow/resources/config" xmlns:bea="http://www.bea.com/bpel/ui/extensions" xmlns:ext="http://www.bea.com/bpel/extensions" xmlns:expr="http://www.bea.com/wli/sb/stages/config" xmlns:trf="http://www.bea.com/wli/sb/stages/transform/config" xmlns:bind="ConsultaTarjetaCreditoCliente" xmlns:ns1="http://alcance.local/wsinfotarjetas/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <bpel:partnerLinks>
        <bpel:partnerLink name="ConsultaTCClienteGT" partnerLinkType="tns:ConsultaTCClienteGT" myRole="ConsultaTCClienteGT">
	       </bpel:partnerLink>
	   <bpel:partnerLink name="transaccionesAlcance" partnerLinkType="tns:transaccionesAlcance_plnkType" partnerRole="transaccionesAlcance_PartnerRole">
	   </bpel:partnerLink></bpel:partnerLinks>

	   <bpel:variables>
        <bpel:variable name="request" messageType="bind:ConsultaTarjetaCreditoClienteRequest">
	       </bpel:variable>
	       <bpel:variable name="response" messageType="bind:ConsultaTarjetaCreditoClienteResponse">
	       </bpel:variable>
    
    
    <bpel:variable name="totalTarjetas" type="xsd:int"/>
    <bpel:variable name="REQConsTarjetas" messageType="ns1:ConsultarTarjetasCredito_v2SoapIn">
    </bpel:variable>
    <bpel:variable name="RSPConsTarjetas" messageType="ns1:ConsultarTarjetasCredito_v2SoapOut">
    </bpel:variable></bpel:variables>

    <bpel:sequence>
        <bpel:receive partnerLink="ConsultaTCClienteGT" operation="ConsultaTarjetaCreditoCliente" variable="request" createInstance="yes">
	           <rescon:receiveInfo>
                
            <rescon:wsdl ref="Middleware/Business_Resources/consultasCliente/consultaPasivosCliente/consultaTC/ConsultaTarjetaCreditoCliente" binding="bind:ConsultaTarjetaCreditoClienteBinding"/></rescon:receiveInfo>
        </bpel:receive>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="REQConsTarjetas.parameters">
        			<trf:expr>
        				<expr:xqueryTransform>
        					<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCCliente/xq/sjConsTarjetasClienteIn"/>
        					
        					
        					
        					
        					<expr:param name="customerId">
        						<expr:path>fn:string($request.inputParameters/bind:PARAMETERS/bind:CUSTOMERID/text())</expr:path></expr:param>
        					<expr:param name="returnBalances">
        						<expr:path>"NONE"</expr:path></expr:param>
        					<expr:param name="queryType">
        						<expr:path>"1"</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign>
        	</bpel:extensionAssignOperation>
        </bpel:assign>
        <bpel:invoke operation="ConsultarTarjetas_v2" partnerLink="transaccionesAlcance" inputVariable="REQConsTarjetas" outputVariable="RSPConsTarjetas">
        	<rescon:invokeInfo><rescon:service ref="Middleware/v2/BusinessServices/transaccionesAlcance/biz/transaccionesAlcance" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
        <bpel:assign>
        	<bpel:extensionAssignOperation>
        		<trf:assign varName="response.outputParameters">
        			<trf:expr>
        				<expr:xqueryText>&lt;bind:ConsultaTarjetaCreditoTypeResponse>
	&lt;bind:CREDIT_CARD_DETAILS>
	&lt;/bind:CREDIT_CARD_DETAILS>
&lt;/bind:ConsultaTarjetaCreditoTypeResponse></expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
        	
        	</bpel:assign>
        <bpel:if>
        	<bpel:condition>string($RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:estatusConfirmacion/text()) = "00"</bpel:condition>
        	<bpel:sequence>
        		<bpel:empty/>
        	<bpel:forEach parallel="yes" counterName="counter">
		<bpel:startCounterValue>number(1)</bpel:startCounterValue>
		<bpel:finalCounterValue>count($RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:Tarjetas/ns1:StrucTarjetasConsulta_v2)</bpel:finalCounterValue>
		<bpel:scope>
			<bpel:variables>
				<bpel:variable name="REQConsSaldoTC" messageType="ns1:ConsultarSaldosVasaSoapIn"/>
				<bpel:variable name="RSPConsSaldoTC" messageType="ns1:ConsultarSaldosVasaSoapOut"/>
				<bpel:variable type="xsd:string" name="cardStatus"/>
				<bpel:variable name="cardCategory" type="xsd:string"/></bpel:variables>
			<bpel:sequence>
				<bpel:assign>
					<bpel:extensionAssignOperation>
						<trf:assign varName="REQConsSaldoTC.parameters">
							<trf:expr>
								<expr:xqueryTransform>
									<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCCliente/xq/sjConsTarjetaIn"/>
									<expr:param name="cardNumber">
										<expr:path>fn:string($RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:Tarjetas/ns1:StrucTarjetasConsulta_v2[xs:int($counter)]/ns1:noTarjetaCredito/text())</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:assign></bpel:extensionAssignOperation>
					<bpel:extensionAssignOperation>
						<trf:assign varName="cardStatus">
							<trf:expr>
								<expr:xqueryText>$RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:Tarjetas/ns1:StrucTarjetasConsulta_v2[xs:int($counter)]/ns1:estatusTarjeta</expr:xqueryText></trf:expr></trf:assign></bpel:extensionAssignOperation>
					<bpel:extensionAssignOperation>
						<trf:assign varName="cardCategory">
							<trf:expr>
								<expr:xqueryText>$RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:Tarjetas/ns1:StrucTarjetasConsulta_v2[xs:int($counter)]/ns1:categoriaTarjeta</expr:xqueryText></trf:expr></trf:assign>
					</bpel:extensionAssignOperation></bpel:assign>
				<bpel:if>
					<bpel:condition>($cardStatus = "20 - TARJETA NORMAL" or $cardStatus = "22 - TARJETA CON ORDEN DE NO RENOVAR") and
($cardCategory = "1" or $cardCategory = "3")</bpel:condition>
					<bpel:sequence>
						<bpel:empty/>
						<bpel:invoke partnerLink="transaccionesAlcance" operation="consultarSaldos" outputVariable="RSPConsSaldoTC" inputVariable="REQConsSaldoTC">
							<rescon:invokeInfo><rescon:service ref="Middleware/v2/BusinessServices/transaccionesAlcance/biz/transaccionesAlcance" xsi:type="ref:BusinessServiceRef" xmlns:ref="http://www.bea.com/wli/sb/reference" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"/></rescon:invokeInfo></bpel:invoke>
						<bpel:assign>
							<bpel:extensionAssignOperation>
								<bpel:documentation/>
								<trf:insert varName="response.outputParameters">
									<trf:location>
										<expr:xpathText>./bind:CREDIT_CARD_DETAILS</expr:xpathText></trf:location>
									<trf:where>last-child</trf:where>
									<trf:expr>
										<expr:xqueryTransform>
											<expr:resource ref="Middleware/v2/BusinessServices/SJS/consultaTCCliente/xq/sjDatosTarjetaOut"/>
											<expr:param name="consultarSaldosVasaResponse1">
												<expr:path>$RSPConsSaldoTC.parameters</expr:path></expr:param></expr:xqueryTransform></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
					<bpel:else>
						<bpel:sequence>
							<bpel:empty/></bpel:sequence></bpel:else></bpel:if></bpel:sequence></bpel:scope></bpel:forEach>
        	<bpel:if>
        		<bpel:condition>count($response.outputParameters/bind:CREDIT_CARD_DETAILS/bind:CREDIT_CARD_DETAIL) > 0</bpel:condition>
        		<bpel:sequence>
        			<bpel:empty/>
        			<bpel:assign>
        				<bpel:extensionAssignOperation>
        					<trf:insert varName="response.outputParameters">
        						<trf:where>first-child</trf:where>
        						<trf:expr>
        							<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>SUCCESS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        		<bpel:else>
        			<bpel:sequence>
        				<bpel:empty/>
        				<bpel:assign>
        					<bpel:extensionAssignOperation>
        						<trf:insert varName="response.outputParameters">
        							<trf:where>first-child</trf:where>
        							<trf:expr>
        								<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>ERROR&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence></bpel:else></bpel:if></bpel:sequence>
        	<bpel:elseif>
        		<bpel:condition>string($RSPConsTarjetas.parameters/ns1:ConsultarTarjetasCredito_v2Result/ns1:estatusConfirmacion/text()) = "97"</bpel:condition>
        		<bpel:sequence>
        			<bpel:empty/>
        		<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
			<trf:expr>
				<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>NO RECORDS&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert>
		</bpel:extensionAssignOperation>
        		</bpel:assign></bpel:sequence>
        	</bpel:elseif>
        	<bpel:else>
        		<bpel:sequence>
        			<bpel:empty/>
        		<bpel:assign>
		<bpel:extensionAssignOperation>
			<trf:insert varName="response.outputParameters">
				<trf:where>first-child</trf:where>
				<trf:expr>
					<expr:xqueryText>&lt;bind:OVERALL_SUCCESS_INDICATOR>ERROR&lt;/bind:OVERALL_SUCCESS_INDICATOR></expr:xqueryText></trf:expr></trf:insert></bpel:extensionAssignOperation></bpel:assign></bpel:sequence>
        	</bpel:else>
        </bpel:if>
        
        
        
        <bpel:reply partnerLink="ConsultaTCClienteGT" operation="ConsultaTarjetaCreditoCliente" variable="response"/>
    </bpel:sequence>
</bpel:process>