<schema targetNamespace="http://xmlns.oracle.com/pcbpel/adapter/db/sp/ObtenerDescripcion" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:db="http://xmlns.oracle.com/pcbpel/adapter/db/sp/ObtenerDescripcion" elementFormDefault="qualified">
