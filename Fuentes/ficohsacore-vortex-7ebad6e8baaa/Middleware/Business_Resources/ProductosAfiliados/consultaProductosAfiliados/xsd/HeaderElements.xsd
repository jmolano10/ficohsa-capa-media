	<xs:complexType name="ResponseHeader">
		<xs:sequence>
			<xs:element name="messageId" type="xs:string" minOccurs="0"/>
			<xs:element name="successIndicator" type="xs:string" minOccurs="1"/>
			<xs:element name="messages" type="xs:string" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RequestHeader" type="auth:RequestHeader" nillable="true"/>
	<xs:element name="ResponseHeader" type="auth:ResponseHeader" nillable="false"/>
